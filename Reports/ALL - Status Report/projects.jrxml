		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="1000"/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="RUMO - PRD"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="INV_ID" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
TIPO,
VALOR_INICIAL,
NOVO_VALOR,
(NOVO_VALOR-VALOR_INICIAL) DELTA
FROM
(
	SELECT 
	  INVI.ID INV_ID,
	  INVI.CODE INV_CODE,
	  INVI.NAME INV_NAME,
	  RIM.ID CR_ID,
	  RIM.RIM_RISK_ISSUE_CODE CR_CODE,
	  RIM.NAME CR_NAME,
	  CR.EFFECT_ON_COST CR_CUSTO,
	  CR.EFFECT_ON_SCHEDULE CR_CRONO,
	  'Custo (R$)' TIPO,
	  (
		SELECT 
			SUM(ROUND((fp_cost.finish_date - fp_cost.start_date)*fp_cost.slice,2))
		FROM 
			INV_INVESTMENTS INV
			LEFT JOIN FIN_PLANS FP ON INV.ID = FP.OBJECT_ID AND FP.PLAN_TYPE_CODE = 'BUDGET'
			LEFT JOIN FIN_COST_PLAN_DETAILS FPD on FP.ID = FPD.PLAN_ID
			LEFT JOIN ODF_SSL_CST_DTL_COST FP_COST ON FPD.ID = FP_COST.PRJ_OBJECT_ID
		WHERE 
			FP.ID = (SELECT ID FROM FIN_PLANS WHERE PLAN_TYPE_CODE = 'BUDGET' AND OBJECT_ID = INVI.ID AND REVISION = ((SELECT FP2.REVISION FROM FIN_PLANS FP2 JOIN ODF_CA_CHANGE OCR2 ON OCR2.ALL_PLANO_ORC = FP2.ID WHERE OCR2.ID = RIM.ID))-1)
			AND FP.OBJECT_ID = INVI.ID
	  )VALOR_INICIAL,
	  (
		SELECT 
			SUM(ROUND((fp_cost.finish_date - fp_cost.start_date)*fp_cost.slice,2))
		FROM 
			INV_INVESTMENTS INV
			LEFT JOIN FIN_PLANS FP ON INV.ID = FP.OBJECT_ID AND FP.PLAN_TYPE_CODE = 'BUDGET'
			LEFT JOIN FIN_COST_PLAN_DETAILS FPD on FP.ID = FPD.PLAN_ID
			LEFT JOIN ODF_SSL_CST_DTL_COST FP_COST ON FPD.ID = FP_COST.PRJ_OBJECT_ID
		WHERE 
			FP.ID = OCR.ALL_PLANO_ORC
			AND FP.OBJECT_ID = INVI.ID
	  )NOVO_VALOR,
	   RIM.IMPACT_DESCRIPTION CR_ESCOPO
	FROM 
	  INV_INVESTMENTS INVI
	  INNER JOIN ODF_CA_INV OCINV ON INVI.ID = OCINV.ID
	  INNER JOIN ODF_CA_PROJECT OCPRJ ON OCPRJ.ID = INVI.ID
	  INNER JOIN RIM_RISKS_AND_ISSUES RIM ON RIM.PK_ID = INVI.ID AND RIM.TYPE_CODE = 'CHANGE'
	  INNER JOIN RIM_CHANGE_REQUESTS CR ON CR.ID = RIM.ID
	  LEFT JOIN ODF_CA_CHANGE OCR ON OCR.ID = RIM.ID
	WHERE 
	  INVI.ID = $P{INV_ID} 
)CUSTO

UNION

SELECT
TIPO,
VALOR_INICIAL,
NOVO_VALOR,
(NOVO_VALOR-VALOR_INICIAL) DELTA
FROM
(
	SELECT 
	  INVI.ID INV_ID,
	  INVI.CODE INV_CODE,
	  INVI.NAME INV_NAME,
	  RIM.ID CR_ID,
	  RIM.RIM_RISK_ISSUE_CODE CR_CODE,
	  RIM.NAME CR_NAME,
	  'Prazo (dias)' TIPO,
	   OCR.all_baseline BASELINE,
		(
			SELECT ROUND(MAX(BASED.FINISH_DATE) - MIN(BASED.START_DATE)) 
			FROM PRJ_BASELINE_DETAILS BASED 
			INNER JOIN PRJ_BASELINES BASE ON BASE.ID = BASED.BASELINE_ID
			WHERE BASED.BASELINE_ID = NVL((
											SELECT MAX(OCR2.all_baseline) 
											FROM ODF_CA_CHANGE OCR2 
											INNER JOIN RIM_RISKS_AND_ISSUES RIM2 ON RIM2.PK_ID = $P{INV_ID} AND RIM2.TYPE_CODE = 'CHANGE'
											WHERE OCR2.all_baseline < OCR.all_baseline
										  ),(SELECT MIN(BASE.ID) FROM PRJ_BASELINES BASE WHERE BASE.PROJECT_ID = $P{INV_ID}))
		)VALOR_INICIAL,
	   (
			SELECT ROUND(MAX(BASED.FINISH_DATE) - MIN(BASED.START_DATE)) 
			FROM PRJ_BASELINE_DETAILS BASED 
			INNER JOIN PRJ_BASELINES BASE ON BASE.ID = BASED.BASELINE_ID
			WHERE BASED.BASELINE_ID = OCR.all_baseline AND BASE.PROJECT_ID = INVI.ID
	   )NOVO_VALOR
	FROM 
	  INV_INVESTMENTS INVI
	  INNER JOIN ODF_CA_INV OCINV ON INVI.ID = OCINV.ID
	  INNER JOIN ODF_CA_PROJECT OCPRJ ON OCPRJ.ID = INVI.ID
	  INNER JOIN RIM_RISKS_AND_ISSUES RIM ON RIM.PK_ID = INVI.ID AND RIM.TYPE_CODE = 'CHANGE'
	  INNER JOIN RIM_CHANGE_REQUESTS CR ON CR.ID = RIM.ID
	  LEFT JOIN ODF_CA_CHANGE OCR ON OCR.ID = RIM.ID
	WHERE 
	  INVI.ID = $P{INV_ID} 
	  AND OCR.all_baseline IS NOT NULL
)PRAZO
WHERE rownum<=4]]>
		</queryString>
		<field name="TIPO" class="java.lang.String"/>
		<field name="VALOR_INICIAL" class="java.math.BigDecimal"/>
		<field name="NOVO_VALOR" class="java.math.BigDecimal"/>
		<field name="DELTA" class="java.math.BigDecimal"/>
	</subDataset>
