	</subDataset>
	<subDataset name="curva_s_fin" uuid="d9cb548a-8640-49bc-8d74-d0b14fde2a3f">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="RUMO - DEV"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="INV_ID" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT  NVL(PERIODO,SYSDATE) PERIODO,
TIPO TIPO,
SUM(NVL(VALOR,0)) OVER (PARTITION BY TIPO ORDER BY PERIODO) VALOR,
RANK() OVER (PARTITION BY 1 ORDER BY PERIODO, ORDEM_TIPO) ordem

FROM
(
SELECT PERIODO,
TIPO,
ORDEM_TIPO,
SUM(VALOR) VALOR


FROM

(
SELECT ID INV_ID
, 'Projeto' TIPO 
, 1 ORDEM_TIPO
, TRUNC(SCHEDULE_START,'MONTH') PERIODO
, NULL VALOR

FROM INV_INVESTMENTS 

WHERE ID = $P{INV_ID}

UNION

SELECT ID INV_ID
, 'Real' TIPO 
, 2 ORDEM_TIPO
, TRUNC(SCHEDULE_START,'MONTH') PERIODO
, NULL VALOR

FROM INV_INVESTMENTS 

WHERE ID = $P{INV_ID}

UNION

SELECT ID INV_ID
, 'Forecast' TIPO 
, 3 ORDEM_TIPO
, TRUNC(SCHEDULE_START,'MONTH') PERIODO
, NULL VALOR

FROM INV_INVESTMENTS 

WHERE ID = $P{INV_ID}

UNION

SELECT ID INV_ID
, 'Projeto + Suplementação' TIPO 
, 4 ORDEM_TIPO
, TRUNC(SCHEDULE_START,'MONTH') PERIODO
, NULL VALOR

FROM INV_INVESTMENTS 

WHERE ID = $P{INV_ID}

UNION

SELECT ID INV_ID
, 'Orçamento' TIPO 
, 5 ORDEM_TIPO
, TRUNC(SCHEDULE_START,'MONTH') PERIODO
, NULL VALOR

FROM INV_INVESTMENTS 

WHERE ID = $P{INV_ID}

UNION

SELECT
INV_ID
, TIPO
, ORDEM_TIPO
, PERIODO
, SUM(VALOR) VALOR

FROM(

SELECT DISTINCT FPLAN.OBJECT_ID INV_ID
, 'Forecast' TIPO 
, 3 ORDEM_TIPO
, TRUNC(NVL(PERIODOS.START_DATE,(SELECT SCHEDULE_FINISH FROM INV_INVESTMENTS WHERE ID = $P{INV_ID})),'MONTH') PERIODO
, NVL(SUM(ROUND((SLICE.FINISH_DATE-SLICE.START_DATE)*SLICE.SLICE,2)),0) VALOR

FROM FIN_PLANS FPLAN
JOIN odf_ca_costplan OFPLAN ON FPLAN.ID = OFPLAN.ID AND OFPLAN.ALL_ATIVO = 1 AND FPLAN.PLAN_TYPE_CODE = 'FORECAST' AND FPLAN.OBJECT_ID = $P{INV_ID}
LEFT JOIN FIN_COST_PLAN_DETAILS FPLAND ON FPLAND.PLAN_ID = FPLAN.ID
JOIN biz_com_periods PER_INI ON FPLAN.START_PERIOD_ID = PER_INI.ID AND PER_INI.PERIOD_TYPE = FPLAN.PERIOD_TYPE_CODE
JOIN biz_com_periods PER_FIM ON FPLAN.END_PERIOD_ID = PER_FIM.ID AND PER_FIM.PERIOD_TYPE = FPLAN.PERIOD_TYPE_CODE
JOIN biz_com_periods PERIODOS ON PERIODOS.START_DATE <= PER_FIM.START_DATE AND PERIODOS.START_DATE >=PER_INI.START_DATE  AND PERIODOS.PERIOD_TYPE = FPLAN.PERIOD_TYPE_CODE
LEFT JOIN ODF_SSL_CST_DTL_COST SLICE ON SLICE.PRJ_OBJECT_ID = FPLAND.ID AND SLICE.START_DATE = PERIODOS.START_DATE
RIGHT JOIN CMN_LOOKUPS_V TIPO_PLANO ON TIPO_PLANO.LOOKUP_CODE = OFPLAN.ALL_TP_PLANO AND TIPO_PLANO.LANGUAGE_CODE = 'pt' AND TIPO_PLANO.LOOKUP_TYPE = 'ALL_LKP_TP_PLANO'

WHERE TIPO_PLANO.LOOKUP_CODE  = 'all_forecast'
AND PERIODOS.START_DATE <= (SELECT SCHEDULE_FINISH FROM INV_INVESTMENTS WHERE ID = $P{INV_ID})
AND PERIODOS.START_DATE >= PER_INI.START_DATE
GROUP BY FPLAN.OBJECT_ID 
, TIPO_PLANO.NAME  
, PERIODOS.START_DATE
, OFPLAN.ALL_CUSTO_TOTAL

UNION

SELECT DISTINCT FPLAN.OBJECT_ID INV_ID
, 'Forecast' TIPO 
, 3 ORDEM_TIPO
, TRUNC(PER_INI.START_DATE-1,'MONTH') PERIODO
, NVL(SUM(ACTUAL.TOTALCOST),NVL(SUM(ROUND((SLICE.FINISH_DATE-SLICE.START_DATE)*SLICE.SLICE,2)),0)) VALOR

FROM FIN_PLANS FPLAN
JOIN odf_ca_costplan OFPLAN ON FPLAN.ID = OFPLAN.ID AND OFPLAN.ALL_ATIVO = 1 AND FPLAN.PLAN_TYPE_CODE = 'FORECAST' AND FPLAN.OBJECT_ID = $P{INV_ID}
LEFT JOIN FIN_COST_PLAN_DETAILS FPLAND ON FPLAND.PLAN_ID = FPLAN.ID
JOIN biz_com_periods PER_INI ON FPLAN.START_PERIOD_ID = PER_INI.ID AND PER_INI.PERIOD_TYPE = FPLAN.PERIOD_TYPE_CODE
LEFT JOIN ODF_SSL_CST_DTL_COST SLICE ON SLICE.PRJ_OBJECT_ID = FPLAND.ID AND SLICE.START_DATE = PER_INI.START_DATE
RIGHT JOIN CMN_LOOKUPS_V TIPO_PLANO ON TIPO_PLANO.LOOKUP_CODE = OFPLAN.ALL_TP_PLANO AND TIPO_PLANO.LANGUAGE_CODE = 'pt' AND TIPO_PLANO.LOOKUP_TYPE = 'ALL_LKP_TP_PLANO'
JOIN PPA_WIP WIP ON WIP.MONTH_BEGIN < PER_INI.START_DATE AND FPLAN.OBJECT_ID = WIP.INVESTMENT_ID
LEFT JOIN PPA_WIP_VALUES ACTUAL ON ACTUAL.TRANSNO = WIP.TRANSNO AND ACTUAL.CURRENCY_TYPE = 'HOME'

WHERE TIPO_PLANO.LOOKUP_CODE  = 'all_forecast'
AND WIP.STATUS = 0
GROUP BY FPLAN.OBJECT_ID 
, PER_INI.START_DATE


UNION

SELECT FPLAN.OBJECT_ID INV_ID
, 'Orçamento' TIPO 
, 5 ORDEM_TIPO
, TRUNC(NVL(PERIODOS.START_DATE,(SELECT SCHEDULE_FINISH FROM INV_INVESTMENTS WHERE ID = $P{INV_ID})),'MONTH') PERIODO
, SUM(ROUND((SLICE.FINISH_DATE-SLICE.START_DATE)*SLICE.SLICE,2)) VALOR

FROM FIN_PLANS FPLAN
JOIN odf_ca_costplan OFPLAN ON FPLAN.ID = OFPLAN.ID AND OFPLAN.ALL_ATIVO = 1 AND FPLAN.PLAN_TYPE_CODE = 'FORECAST' AND FPLAN.OBJECT_ID = $P{INV_ID}
JOIN FIN_COST_PLAN_DETAILS FPLAND ON FPLAND.PLAN_ID = FPLAN.ID
JOIN biz_com_periods PER_INI ON FPLAN.START_PERIOD_ID = PER_INI.ID AND PER_INI.PERIOD_TYPE = FPLAN.PERIOD_TYPE_CODE
JOIN biz_com_periods PER_FIM ON FPLAN.END_PERIOD_ID = PER_FIM.ID AND PER_FIM.PERIOD_TYPE = FPLAN.PERIOD_TYPE_CODE
JOIN biz_com_periods PERIODOS ON PERIODOS.START_DATE <= PER_FIM.START_DATE AND PERIODOS.START_DATE >=PER_INI.START_DATE AND PERIODOS.PERIOD_TYPE = FPLAN.PERIOD_TYPE_CODE
LEFT JOIN ODF_SSL_CST_DTL_COST SLICE ON SLICE.PRJ_OBJECT_ID = FPLAND.ID AND SLICE.START_DATE = PERIODOS.START_DATE
RIGHT JOIN CMN_LOOKUPS_V TIPO_PLANO ON TIPO_PLANO.LOOKUP_CODE = OFPLAN.ALL_TP_PLANO AND TIPO_PLANO.LANGUAGE_CODE = 'pt' AND TIPO_PLANO.LOOKUP_TYPE = 'ALL_LKP_TP_PLANO'

WHERE TIPO_PLANO.LOOKUP_CODE  = 'all_orcamento'
AND PERIODOS.START_DATE <= (SELECT SCHEDULE_FINISH FROM INV_INVESTMENTS WHERE ID = $P{INV_ID})
AND PERIODOS.START_DATE >= PER_INI.START_DATE

GROUP BY FPLAN.OBJECT_ID 
, TIPO_PLANO.NAME  
, PERIODOS.START_DATE 
, OFPLAN.ALL_CUSTO_TOTAL

UNION


SELECT FPLAN.OBJECT_ID INV_ID
, 'Projeto + Suplementação' TIPO 
, 4 ORDEM_TIPO
, TRUNC(NVL(SLICE.START_DATE,(SELECT SCHEDULE_FINISH FROM INV_INVESTMENTS WHERE ID = $P{INV_ID})),'MONTH') PERIODO
, SUM(ROUND((SLICE.FINISH_DATE-SLICE.START_DATE)*SLICE.SLICE,2)) VALOR

FROM FIN_PLANS FPLAN
JOIN odf_ca_costplan OFPLAN ON FPLAN.ID = OFPLAN.ID AND OFPLAN.ALL_ATIVO = 1 AND FPLAN.PLAN_TYPE_CODE = 'FORECAST' AND FPLAN.OBJECT_ID = $P{INV_ID}
JOIN FIN_COST_PLAN_DETAILS FPLAND ON FPLAND.PLAN_ID = FPLAN.ID
JOIN ODF_SSL_CST_DTL_COST SLICE ON SLICE.PRJ_OBJECT_ID = FPLAND.ID
RIGHT JOIN CMN_LOOKUPS_V TIPO_PLANO ON TIPO_PLANO.LOOKUP_CODE = OFPLAN.ALL_TP_PLANO AND TIPO_PLANO.LANGUAGE_CODE = 'pt' AND TIPO_PLANO.LOOKUP_TYPE = 'ALL_LKP_TP_PLANO'

WHERE TIPO_PLANO.LOOKUP_CODE  = 'all_prj_suplementacao'

GROUP BY FPLAN.OBJECT_ID 
, TIPO_PLANO.NAME  
, SLICE.START_DATE 
, OFPLAN.ALL_CUSTO_TOTAL

UNION


SELECT INV_ID
, 'Projeto' TIPO 
, 1 ORDEM_TIPO
, MES PERIODO
, 0 VALOR
  FROM 
(
SELECT INV_ID
,ADD_MONTHS ( start_dt , LEVEL - 1) MES
FROM     (SELECT INV_ID
          ,  TRUNC ( START_DATE, 'MONTH') AS start_dt
,TRUNC ( FINISH_DATE, 'MONTH') AS end_dt FROM 

(
SELECT FPLAN.OBJECT_ID INV_ID
, MIN(SLICE.START_DATE) START_DATE
, MAX(SLICE.START_DATE) FINISH_DATE

FROM FIN_PLANS FPLAN
JOIN odf_ca_costplan OFPLAN ON FPLAN.ID = OFPLAN.ID
LEFT JOIN CMN_LOOKUPS_V TIPO_PLANO ON TIPO_PLANO.LOOKUP_CODE = OFPLAN.ALL_TP_PLANO AND TIPO_PLANO.LANGUAGE_CODE = 'pt' AND TIPO_PLANO.LOOKUP_TYPE = 'ALL_LKP_TP_PLANO'
JOIN FIN_COST_PLAN_DETAILS FPLAND ON FPLAND.PLAN_ID = FPLAN.ID
JOIN ODF_SSL_CST_DTL_COST SLICE ON SLICE.PRJ_OBJECT_ID = FPLAND.ID

WHERE OFPLAN.ALL_ATIVO = 1
AND FPLAN.PLAN_TYPE_CODE = 'FORECAST'
AND FPLAN.OBJECT_ID = $P{INV_ID}

GROUP BY FPLAN.OBJECT_ID 
)
)
CONNECT BY     LEVEL <= 1 + MONTHS_BETWEEN ( end_dt, start_dt)
)
)PLANEJADO
GROUP BY INV_ID, TIPO, PERIODO, ORDEM_TIPO


UNION

SELECT
INV_ID
, 'Projeto' TIPO
, 1 ORDEM_TIPO
, TRUNC(PERIODO,'MONTH') PERIODO
, SUM(VALOR)

FROM(
SELECT FPLAN.OBJECT_ID INV_ID
, SLICE.START_DATE PERIODO
, SUM(ROUND((SLICE.FINISH_DATE-SLICE.START_DATE)*SLICE.SLICE,2)) VALOR

FROM FIN_PLANS FPLAN
JOIN odf_ca_costplan OFPLAN ON FPLAN.ID = OFPLAN.ID
LEFT JOIN CMN_LOOKUPS_V TIPO_PLANO ON TIPO_PLANO.LOOKUP_CODE = OFPLAN.ALL_TP_PLANO AND TIPO_PLANO.LANGUAGE_CODE = 'pt' AND TIPO_PLANO.LOOKUP_TYPE = 'ALL_LKP_TP_PLANO'
JOIN FIN_COST_PLAN_DETAILS FPLAND ON FPLAND.PLAN_ID = FPLAN.ID
JOIN ODF_SSL_CST_DTL_COST SLICE ON SLICE.PRJ_OBJECT_ID = FPLAND.ID

WHERE FPLAN.IS_PLAN_OF_RECORD = 1
AND FPLAN.PLAN_TYPE_CODE = 'BUDGET'
AND FPLAN.OBJECT_ID = $P{INV_ID}

GROUP BY FPLAN.OBJECT_ID 
, TIPO_PLANO.NAME  
, SLICE.START_DATE 

UNION

SELECT INV_ID
, MES PERIODO
, 0 VALOR
  FROM 
(
SELECT INV_ID
,ADD_MONTHS ( start_dt , LEVEL - 1) MES
FROM     (SELECT INV_ID
          ,  TRUNC ( START_DATE, 'MONTH') AS start_dt
,TRUNC ( FINISH_DATE, 'MONTH') AS end_dt FROM 

(
SELECT FPLAN.OBJECT_ID INV_ID
, MIN(SLICE.START_DATE) START_DATE
, MAX(SLICE.START_DATE) FINISH_DATE

FROM FIN_PLANS FPLAN
JOIN odf_ca_costplan OFPLAN ON FPLAN.ID = OFPLAN.ID
LEFT JOIN CMN_LOOKUPS_V TIPO_PLANO ON TIPO_PLANO.LOOKUP_CODE = OFPLAN.ALL_TP_PLANO AND TIPO_PLANO.LANGUAGE_CODE = 'pt' AND TIPO_PLANO.LOOKUP_TYPE = 'ALL_LKP_TP_PLANO'
JOIN FIN_COST_PLAN_DETAILS FPLAND ON FPLAND.PLAN_ID = FPLAN.ID
JOIN ODF_SSL_CST_DTL_COST SLICE ON SLICE.PRJ_OBJECT_ID = FPLAND.ID

WHERE FPLAN.IS_PLAN_OF_RECORD = 1
AND FPLAN.PLAN_TYPE_CODE = 'BUDGET'
AND FPLAN.OBJECT_ID = $P{INV_ID}

GROUP BY FPLAN.OBJECT_ID 
)
)
CONNECT BY     LEVEL <= 1 + MONTHS_BETWEEN ( end_dt, start_dt)
)
)ORCAMENTO
GROUP BY INV_ID, PERIODO

UNION 

SELECT
INV_ID
, 'Real' TIPO
, 2 ORDEM_TIPO
, TRUNC(PERIODO,'MONTH') PERIODO
, SUM(NVL(VALOR,0))

FROM(

SELECT WIP.INVESTMENT_ID INV_ID
, WIP.MONTH_BEGIN PERIODO
, SUM(ACTUAL.TOTALCOST) VALOR

FROM PPA_WIP WIP 
LEFT JOIN PPA_WIP_VALUES ACTUAL ON ACTUAL.TRANSNO = WIP.TRANSNO AND ACTUAL.CURRENCY_TYPE = 'HOME'

WHERE WIP.INVESTMENT_ID = $P{INV_ID}
AND WIP.STATUS = 0
AND TRUNC(WIP.TRANSDATE,'MONTH') <= (
SELECT 
TRUNC(MAX(PER_INI.START_DATE),'MONTH')
FROM FIN_PLANS FPLAN
JOIN odf_ca_costplan OFPLAN ON FPLAN.ID = OFPLAN.ID AND OFPLAN.ALL_ATIVO = 1 AND FPLAN.PLAN_TYPE_CODE = 'FORECAST' AND FPLAN.OBJECT_ID = $P{INV_ID}
JOIN FIN_COST_PLAN_DETAILS FPLAND ON FPLAND.PLAN_ID = FPLAN.ID
JOIN biz_com_periods PER_INI ON FPLAN.START_PERIOD_ID = PER_INI.ID AND PER_INI.PERIOD_TYPE = FPLAN.PERIOD_TYPE_CODE
WHERE OFPLAN.ALL_TP_PLANO = 'all_forecast'
)

GROUP BY WIP.INVESTMENT_ID 
, WIP.MONTH_BEGIN

UNION
--Obter todas as datas
SELECT INV_ID
, MES PERIODO
, 0 VALOR
  FROM 
(
SELECT INV_ID
,ADD_MONTHS ( start_dt , LEVEL - 1) MES
FROM     (SELECT INV_ID
          ,  TRUNC ( START_DATE, 'MONTH') AS start_dt
,TRUNC ( FINISH_DATE, 'MONTH') AS end_dt FROM 

(
SELECT WIP.INVESTMENT_ID INV_ID
, MIN(WIP.MONTH_BEGIN) AS START_DATE
, MAX(WIP.MONTH_BEGIN) FINISH_DATE

FROM PPA_WIP WIP 
LEFT JOIN PPA_WIP_VALUES ACTUAL ON ACTUAL.TRANSNO = WIP.TRANSNO AND ACTUAL.CURRENCY_TYPE = 'HOME'

WHERE WIP.INVESTMENT_ID = $P{INV_ID}

GROUP BY WIP.INVESTMENT_ID 
)
)
CONNECT BY     LEVEL <= 1 + MONTHS_BETWEEN ( end_dt, start_dt)
)
) REALIZADO

GROUP BY INV_ID, PERIODO

) VALORES
GROUP BY
PERIODO ,
TIPO , 
ORDEM_TIPO
) TOTAL

WHERE 1=1]]>
