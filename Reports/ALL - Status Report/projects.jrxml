	<subDataset name="riscos" uuid="b787b70a-2f4b-48cb-b4ab-f2676117a198">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="RUMO - DEV"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="INV_ID" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT 
  SUBSTR(RISCO.NAME,1,100) RISCO,
  SUBSTR(RISK.RESOLUTION,1,100) ACAO
FROM 
  RIM_RISKS_AND_ISSUES RISK
  JOIN ODF_CA_RISK ORISK ON ORISK.ID = RISK.ID
  JOIN CMN_LOOKUPS_V RISCO ON ORISK.all_riscos_agrup = RISCO.LOOKUP_CODE AND RISCO.LOOKUP_TYPE = 'ALL_LKP_RISCOS_AGRUP' AND RISCO.LANGUAGE_CODE = 'pt'
WHERE 
  RISK.TYPE_CODE = 'RISK'
  AND RISK.PK_ID = $P{INV_ID}
  AND ROWNUM <= 3]]>
		</queryString>
		<field name="RISCO" class="java.lang.String"/>
		<field name="ACAO" class="java.lang.String"/>
	</subDataset>
	<subDataset name="marcos" uuid="854801fd-393a-4a68-80e6-812588786969">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="RUMO - DEV"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="INV_ID" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT 
	t.prid milestone_int_id,
	t.prwbssequence wbs_sequence,
	t.prexternalid milestone_id,
	t.prid taskid,
	t.prprojectid investment_id,
	SUBSTR(t.prname,1,40) milestone_name,
	t.prismilestone ismilestone,   
	t.prstart start_date,
	t.prfinish finish_date,
	baseline.start_date baseline_start,
	baseline.finish_date baseline_finish,    
	tsk_d.days_late lateness,
	tsk_d.days_late lateness_sl,
	tsk_d.days_late_pct days_late_pct,
	tsk_d.days_late_pct*100 days_late_pct_sl

FROM 
	PRTASK t   
	INNER JOIN COP_TSK_DAYS_LATE_V tsk_d ON t.prid = tsk_d.task_id 
	LEFT OUTER JOIN PRJ_BASELINE_DETAILS baseline ON baseline.object_id = t.prid AND UPPER(baseline.object_type) = 'TASK' AND baseline.is_current = 1

WHERE  
	t.prismilestone = 1
	AND t.prstatus !=2
	AND ROWNUM <= 7
	AND t.prprojectid =  $P{INV_ID}]]>
		</queryString>
		<field name="MILESTONE_INT_ID" class="java.math.BigDecimal"/>
		<field name="WBS_SEQUENCE" class="java.math.BigDecimal"/>
		<field name="MILESTONE_ID" class="java.lang.String"/>
		<field name="TASKID" class="java.math.BigDecimal"/>
		<field name="INVESTMENT_ID" class="java.math.BigDecimal"/>
		<field name="MILESTONE_NAME" class="java.lang.String"/>
		<field name="ISMILESTONE" class="java.math.BigDecimal"/>
		<field name="START_DATE" class="java.sql.Timestamp"/>
		<field name="FINISH_DATE" class="java.sql.Timestamp"/>
		<field name="BASELINE_START" class="java.sql.Timestamp"/>
		<field name="BASELINE_FINISH" class="java.sql.Timestamp"/>
		<field name="LATENESS" class="java.math.BigDecimal"/>
		<field name="LATENESS_SL" class="java.math.BigDecimal"/>
		<field name="DAYS_LATE_PCT" class="java.math.BigDecimal"/>
		<field name="DAYS_LATE_PCT_SL" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="curva_s_fisica" uuid="d7c8e96a-e6a0-4179-bff4-f94144c55465">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="RUMO ALL - DEV"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="INV_ID" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT 
	ALL_MES MES,
