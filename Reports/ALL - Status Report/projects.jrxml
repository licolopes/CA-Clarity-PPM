	<parameter name="projeto" class="java.util.Collection"/>
	<queryString>
		<![CDATA[SELECT 
	PRJ.ODF_PK INV_ID
	, PRJ.UNIQUE_CODE INV_CODE
	, SUBSTR(PRJ.NAME,1,70) INV_NAME
	, PRJ.SCHEDULE_START DATA_INI
	, PRJ.SCHEDULE_FINISH DATA_FIM
	, NVL(ROUND(PRJ.all_pct_planejado*100),0) PCT_PLAN
	, NVL(ROUND(PRJ.all_pct_conc*100),0) PCT_CONC
	, MAN.FIRST_NAME || ' ' || MAN.LAST_NAME GERENTE
	, AREA.NAME AREA
	, STATUS_RPT.COP_REPORT_DATE RPT_DATE
	, STATUS_RPT.cop_key_accomplish PRINC_REAL
	, STATUS_RPT.cop_report_update PONTOS_CRIT
	, STATUS_RPT.cop_upcoming_act PROX_ATIV
	, EXE.NAME ORG
	, NVL(FIN.BUDGET_CST_TOTAL,0) VALOR
	, NVL((SELECT COUNT(RIM.PK_ID) FROM RIM_RISKS_AND_ISSUES RIM WHERE RIM.PK_ID = PRJ.ODF_PK AND RIM.TYPE_CODE = 'CHANGE'),0) QTD_CR

FROM 
	ODF_PROJECT_V2 PRJ
	LEFT JOIN ODF_OBJECT_INSTANCE_MAPPING MAP ON MAP.PRIMARY_OBJECT_INSTANCE_ID = PRJ.ODF_PK
	LEFT JOIN FIN_FINANCIALS FIN ON MAP.SECONDARY_OBJECT_INSTANCE_ID = FIN.ID
	JOIN SRM_RESOURCES MAN ON PRJ.rumo_gerente_exib = MAN.USER_ID
	JOIN CMN_LOOKUPS_V AREA ON PRJ.all_area = AREA.LOOKUP_CODE AND AREA.LOOKUP_TYPE = 'ALL_LKP_AREAS' AND AREA.LANGUAGE_CODE = 'pt'
	LEFT JOIN ODF_CA_COP_PRJ_STATUSRPT STATUS_RPT ON STATUS_RPT.ODF_PARENT_ID = PRJ.ODF_PK
	LEFT JOIN PRJ_OBS_ASSOCIATIONS OBS_ASS ON OBS_ASS.RECORD_ID = PRJ.ODF_PK AND OBS_ASS.TABLE_NAME = 'SRM_PROJECTS'
	LEFT JOIN PRJ_OBS_UNITS EXE ON EXE.ID = OBS_ASS.UNIT_ID

WHERE 
	EXE.NAME IS NOT NULL
	AND (STATUS_RPT.ID = (SELECT MAX(RPT.ID) FROM ODF_CA_COP_PRJ_STATUSRPT RPT WHERE RPT.ODF_PARENT_ID = PRJ.ODF_PK) OR STATUS_RPT.ID IS NULL)
	AND PRJ.ALL_AREA =  $P{operacao} 
	AND EXE.TYPE_ID = $P{tipo_org}
	AND $X{IN,EXE.ID,org_exec}
	AND $X{IN,PRJ.RUMO_GERENTE_EXIB,ger_proj}
	AND $X{IN,PRJ.OBJ_REQUEST_CATEGORY,class}
	AND $X{IN,PRJ.STAGE_CODE,status}
	AND $X{IN,PRJ.MANAGER,adm_licencas}
	AND $X{IN,PRJ.ODF_PK,projeto}]]>
	</queryString>
