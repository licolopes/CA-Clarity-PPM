	<subDataset name="RISK_OPEN" uuid="f802535c-e126-4108-bfff-cfb707fd072a">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="UNIMED - PRD"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="0"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="1000"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="tipo_proj" class="java.util.Collection"/>
		<parameter name="dir_resp" class="java.util.Collection"/>
		<parameter name="area_resp" class="java.util.Collection"/>
		<queryString>
			<![CDATA[SELECT
	INV_ID,
	INV_CODE,
	INV_NAME,
	COUNT(INV_ID) QTD
FROM
(
  SELECT DISTINCT
    INV.ID INV_ID,
    INV.CODE INV_CODE,
    INV.NAME INV_NAME,
    RIM.ID RISCO_ID,  
    RIM.RIM_RISK_ISSUE_CODE RISCO_CODE,
    RIM.NAME RISCO_NAME,
	TP.NAME TP_NAME,
	TP.LOOKUP_CODE TP_CODE,
	DAS.FIRST_NAME || ' ' || DAS.LAST_NAME DIRETOR_NAME,
	DAS.UNIQUE_NAME DIRETOR_CODE,
	OBSU.NAME AREA_CLIENTE  
  FROM
    INV_INVESTMENTS INV
	INNER JOIN INV_PROJECTS INVP ON INVP.PRID = INV.ID
	INNER JOIN ODF_CA_PROJECT OPRJ ON OPRJ.ID = INV.ID
    INNER JOIN RIM_RISKS_AND_ISSUES RIM ON RIM.PK_ID = INV.ID AND RIM.TYPE_CODE = 'RISK'
	LEFT JOIN CMN_LOOKUPS_V TP ON TP.LOOKUP_CODE = OPRJ.obj_request_type AND TP.LOOKUP_TYPE = 'OBJ_IDEA_PROJECT_TYPE' AND TP.LANGUAGE_CODE = 'pt'
	LEFT JOIN SRM_RESOURCES DAS ON DAS.ID = OPRJ.obj_stakeholder1
	LEFT JOIN PRJ_OBS_ASSOCIATIONS OBSA ON OBSA.RECORD_ID = INV.ID
	JOIN PRJ_OBS_UNITS_FLAT OBSF ON OBSA.UNIT_ID = OBSF.UNIT_ID
	JOIN PRJ_OBS_UNITS OBSU ON OBSU.ID = OBSF.BRANCH_UNIT_ID AND OBSU.DEPTH = (
																				SELECT MAX(OBSU2.DEPTH) 
																				FROM PRJ_OBS_ASSOCIATIONS OBSA2
																				LEFT JOIN PRJ_OBS_UNITS_FLAT OBSF2 ON OBSA2.UNIT_ID = OBSF2.UNIT_ID
																				LEFT JOIN PRJ_OBS_UNITS OBSU2 ON OBSU2.ID = OBSF2.BRANCH_UNIT_ID
																				WHERE OBSA2.RECORD_ID = INV.ID 
																			)
	JOIN PRJ_OBS_OBJECT_TYPES OOT ON OOT.TYPE_ID = OBSU.TYPE_ID AND OOT.TABLE_NAME = 'SRM_PROJECTS' AND OOT.TYPE_ID = 5000003
  WHERE
  	INV.IS_ACTIVE = 1
    AND INVP.IS_TEMPLATE = 0
	AND RIM.STATUS_CODE = 'OPEN'	
)RISCO

WHERE
	$X{IN,TP_CODE,tipo_proj} 
	AND $X{IN,DIRETOR_CODE,dir_resp} 
	AND $X{IN,AREA_CLIENTE,area_resp} 
	
GROUP BY
	INV_ID,
	INV_CODE,
	INV_NAME]]>
		</queryString>
		<field name="INV_ID" class="java.math.BigDecimal"/>
		<field name="INV_CODE" class="java.lang.String"/>
		<field name="INV_NAME" class="java.lang.String"/>
		<field name="QTD" class="java.math.BigDecimal"/>
		<sortField name="QTD" order="Descending"/>
	</subDataset>
