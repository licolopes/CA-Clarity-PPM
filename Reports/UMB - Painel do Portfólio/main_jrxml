	<subDataset name="PROJ_PRAZO" uuid="153d4e38-0111-454a-a06d-07f5a2d6f615">
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="0"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="1000"/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="UNIMED - PRD"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="tipo_proj" class="java.util.Collection"/>
		<parameter name="dir_resp" class="java.util.Collection"/>
		<parameter name="area_resp" class="java.util.Collection"/>
		<queryString>
			<![CDATA[SELECT
DIF,
COUNT(INV_ID) QTD
FROM
(
	SELECT DISTINCT
	  INV.ID INV_ID,
	  INV.CODE INV_CODE,
	  INV.NAME INV_NAME,
	  TO_DATE(MIN(TSK.PRSTART),'DD/MM/YYYY') TSK_START,
	  TO_DATE(MAX(TSK.PRFINISH),'DD/MM/YYYY') TSK_FINISH,
	  TO_DATE(BLD.START_DATE,'DD/MM/YYYY') BL_START,
	  TO_DATE(BLD.FINISH_DATE,'DD/MM/YYYY') BL_FINISH,
	  CASE
		WHEN (TO_DATE(MAX(TSK.PRFINISH),'DD/MM/YYYY') - TO_DATE(BLD.FINISH_DATE,'DD/MM/YYYY')) > 0 THEN 'Fora do prazo'
		ELSE 'No prazo'
		END DIF,
	  TP.NAME TP_NAME,
	  TP.LOOKUP_CODE TP_CODE,
	  DAS.FIRST_NAME || ' ' || DAS.LAST_NAME DIRETOR_NAME,
	  DAS.UNIQUE_NAME DIRETOR_CODE,
	  OBSU.NAME AREA_CLIENTE
	FROM
	  INV_INVESTMENTS INV
	  INNER JOIN INV_PROJECTS INVP ON INVP.PRID = INV.ID
	  INNER JOIN ODF_CA_PROJECT OPRJ ON OPRJ.ID = INV.ID
	  INNER JOIN PRTASK TSK ON TSK.PRPROJECTID = INV.ID
	  INNER JOIN PRJ_BASELINES BL ON BL.PROJECT_ID = INV.ID AND BL.IS_CURRENT = 1
	  INNER JOIN PRJ_BASELINE_DETAILS BLD ON BLD.BASELINE_ID = BL.ID AND BLD.OBJECT_TYPE = 'PROJECT'
	  LEFT JOIN CMN_LOOKUPS_V TP ON TP.LOOKUP_CODE = OPRJ.obj_request_type AND TP.LOOKUP_TYPE = 'OBJ_IDEA_PROJECT_TYPE' AND TP.LANGUAGE_CODE = 'pt'
	  LEFT JOIN SRM_RESOURCES DAS ON DAS.ID = OPRJ.obj_stakeholder1
	  LEFT JOIN PRJ_OBS_ASSOCIATIONS OBSA ON OBSA.RECORD_ID = INV.ID
	  JOIN PRJ_OBS_UNITS_FLAT OBSF ON OBSA.UNIT_ID = OBSF.UNIT_ID
	  JOIN PRJ_OBS_UNITS OBSU ON OBSU.ID = OBSF.BRANCH_UNIT_ID AND OBSU.DEPTH = (
																					SELECT MAX(OBSU2.DEPTH) 
																					FROM PRJ_OBS_ASSOCIATIONS OBSA2
																					LEFT JOIN PRJ_OBS_UNITS_FLAT OBSF2 ON OBSA2.UNIT_ID = OBSF2.UNIT_ID
																					LEFT JOIN PRJ_OBS_UNITS OBSU2 ON OBSU2.ID = OBSF2.BRANCH_UNIT_ID
																					WHERE OBSA2.RECORD_ID = INV.ID 
																				)
	  JOIN PRJ_OBS_OBJECT_TYPES OOT ON OOT.TYPE_ID = OBSU.TYPE_ID AND OOT.TABLE_NAME = 'SRM_PROJECTS' AND OOT.TYPE_ID = 5000003
	WHERE
	  INV.IS_ACTIVE = 1
	  AND INVP.IS_TEMPLATE = 0
	GROUP BY
	  INV.ID,
	  INV.CODE,
	  INV.NAME,
	  BLD.START_DATE,
	  BLD.FINISH_DATE,
	  TP.NAME,
	  TP.LOOKUP_CODE,
	  DAS.FIRST_NAME,
	  DAS.LAST_NAME,
	  DAS.UNIQUE_NAME,
	  OBSU.NAME
)AMTS

WHERE
	$X{IN,TP_CODE,tipo_proj} 
	AND $X{IN,DIRETOR_CODE,dir_resp} 
	AND $X{IN,AREA_CLIENTE,area_resp} 
	
GROUP BY
	DIF]]>
		</queryString>
		<field name="DIF" class="java.lang.String"/>
		<field name="QTD" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="PROJ_SAUDAVEIS" uuid="106cee2f-a83d-47c6-bb97-87ed44a71d86">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="UNIMED - PRD"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="0"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="1000"/>
		<parameter name="tipo_proj" class="java.util.Collection"/>
		<parameter name="dir_resp" class="java.util.Collection"/>
		<parameter name="area_resp" class="java.util.Collection"/>
		<queryString>
			<![CDATA[SELECT
INV_STATUS,
COUNT(INV_ID) QTD
FROM
(
	SELECT
		INV.ID INV_ID,
		INV.CODE INV_CODE,
		INV.NAME INV_NAME,
		NVL(IND.NAME,'Em branco') INV_STATUS,
		TP.NAME TP_NAME,
		TP.LOOKUP_CODE TP_CODE,
		DAS.FIRST_NAME || ' ' || DAS.LAST_NAME DIRETOR_NAME,
		DAS.UNIQUE_NAME DIRETOR_CODE,
		OBSU.NAME AREA_CLIENTE  
	FROM
		INV_INVESTMENTS INV
		INNER JOIN INV_PROJECTS INVP ON INVP.PRID = INV.ID
		INNER JOIN ODF_CA_PROJECT OPRJ ON OPRJ.ID = INV.ID
		LEFT JOIN CMN_LOOKUPS_V IND ON IND.LOOKUP_ENUM = INV.status_indicator AND IND.LOOKUP_TYPE = 'INVESTMENT_STATUS_INDICATOR' AND IND.LANGUAGE_CODE = 'pt'		
		LEFT JOIN CMN_LOOKUPS_V TP ON TP.LOOKUP_CODE = OPRJ.obj_request_type AND TP.LOOKUP_TYPE = 'OBJ_IDEA_PROJECT_TYPE' AND TP.LANGUAGE_CODE = 'pt'
		LEFT JOIN SRM_RESOURCES DAS ON DAS.ID = OPRJ.obj_stakeholder1
		LEFT JOIN PRJ_OBS_ASSOCIATIONS OBSA ON OBSA.RECORD_ID = INV.ID
		JOIN PRJ_OBS_UNITS_FLAT OBSF ON OBSA.UNIT_ID = OBSF.UNIT_ID
		JOIN PRJ_OBS_UNITS OBSU ON OBSU.ID = OBSF.BRANCH_UNIT_ID AND OBSU.DEPTH = (
																					SELECT MAX(OBSU2.DEPTH) 
																					FROM PRJ_OBS_ASSOCIATIONS OBSA2
																					LEFT JOIN PRJ_OBS_UNITS_FLAT OBSF2 ON OBSA2.UNIT_ID = OBSF2.UNIT_ID
																					LEFT JOIN PRJ_OBS_UNITS OBSU2 ON OBSU2.ID = OBSF2.BRANCH_UNIT_ID
																					WHERE OBSA2.RECORD_ID = INV.ID 
																				)
		JOIN PRJ_OBS_OBJECT_TYPES OOT ON OOT.TYPE_ID = OBSU.TYPE_ID AND OOT.TABLE_NAME = 'SRM_PROJECTS' AND OOT.TYPE_ID = 5000003
	WHERE
		INV.IS_ACTIVE = 1
		AND INVP.IS_TEMPLATE = 0
)AMTS

WHERE
  $X{IN,TP_CODE,tipo_proj} 
  AND $X{IN,DIRETOR_CODE,dir_resp} 
  AND $X{IN,AREA_CLIENTE,area_resp} 
  
GROUP BY
  INV_STATUS]]>
		</queryString>
		<field name="INV_STATUS" class="java.lang.String"/>
		<field name="QTD" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="TSK_TEMPO" uuid="223903d4-bc60-488e-b6f4-d3d716d9e354">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="UNIMED - PRD"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="0"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="1000"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.property.dataset.dialog.DatasetDialog.sash.w1" value="598"/>
		<property name="com.jaspersoft.studio.property.dataset.dialog.DatasetDialog.sash.w2" value="388"/>
		<parameter name="tipo_proj" class="java.util.Collection"/>
		<parameter name="dir_resp" class="java.util.Collection"/>
		<parameter name="area_resp" class="java.util.Collection"/>
		<queryString>
			<![CDATA[SELECT
DIF,
ORDEM,
COUNT(INV_ID) QTD
FROM
(
	SELECT DISTINCT
	INV_ID,
	DIF,
	ORDEM
	FROM
	(
		SELECT
		T3.INV_ID,
		T3.INV_CODE,
		T3.INV_NAME,
		T3.TSK_TOTAL,
		T3.TSK_ATRASADAS,
		T3.DIF_PERC,
		T3.DIF,
		CASE
		  WHEN T3.DIF = '> 1% e <= 10%' THEN 10
		  WHEN T3.DIF = '> 10%  e <= 25%' THEN 20
		  WHEN T3.DIF = '> 25%  e <= 50%' THEN 30
		  WHEN T3.DIF = '> 50%  e <= 75%' THEN 40
		  WHEN T3.DIF = '> 75%' THEN 50
		  END ORDEM,
		TASK.TP_CODE,
		TASK.TP_NAME,
		TASK.DIRETOR_CODE,
		TASK.DIRETOR_NAME,
		TASK.AREA_CLIENTE,
		TASK.TSK_ID,
		TASK.TSK_NAME,
		TASK.TSK_START,
		TASK.TSK_FINISH,
		TASK.BL_START,
		TASK.BL_FINISH,
		TASK.TSK_STATUS,
		(TO_DATE(TASK.TSK_FINISH,'DD/MM/YYYY') - TO_DATE(TASK.BL_FINISH,'DD/MM/YYYY')) DIAS
		FROM
		(
			SELECT
			  INV_ID,
			  INV_CODE,
			  INV_NAME,
			  TSK_TOTAL,
			  TSK_ATRASADAS,
			  ROUND((TSK_ATRASADAS/TSK_TOTAL),2) DIF_PERC,
			  CASE
				WHEN ROUND((TSK_ATRASADAS/TSK_TOTAL),2) > 0 AND ROUND((TSK_ATRASADAS/TSK_TOTAL),2) <= 0.10 THEN '> 1% e <= 10%'
				WHEN ROUND((TSK_ATRASADAS/TSK_TOTAL),2) > 0.10 AND ROUND((TSK_ATRASADAS/TSK_TOTAL),2) <= 0.25 THEN '> 10%  e <= 25%'
				WHEN ROUND((TSK_ATRASADAS/TSK_TOTAL),2) > 0.25 AND ROUND((TSK_ATRASADAS/TSK_TOTAL),2) <= 0.50 THEN '> 25%  e <= 50%'
				WHEN ROUND((TSK_ATRASADAS/TSK_TOTAL),2) > 0.50 AND ROUND((TSK_ATRASADAS/TSK_TOTAL),2) <= 0.75 THEN '> 50%  e <= 75%'
				WHEN ROUND((TSK_ATRASADAS/TSK_TOTAL),2) > 0.75 THEN '> 75%'
				END DIF	  
			FROM
			(
			  SELECT
				INV_ID,
				INV_CODE,
				INV_NAME,
				(SELECT COUNT(PRID) FROM PRTASK TSK2 WHERE TSK2.PRPROJECTID = INV_ID AND TSK2.PRISTASK = 1) TSK_TOTAL,
				SUM(TSK_ATRASADAS) TSK_ATRASADAS
			  FROM
			  (
				SELECT DISTINCT
				  INV.ID INV_ID,
				  INV.CODE INV_CODE,
				  INV.NAME INV_NAME,
				  TSK.PRNAME TSK_NAME,
				  STATUS.NAME TSK_STATUS,
				  TO_DATE(TSK.PRSTART,'DD/MM/YYYY') TSK_START,
				  TO_DATE(TSK.PRFINISH,'DD/MM/YYYY') TSK_FINISH,
				  TO_DATE(BLD.START_DATE,'DD/MM/YYYY') BL_START,
				  TO_DATE(BLD.FINISH_DATE,'DD/MM/YYYY') BL_FINISH,
				  CASE
					WHEN (TO_DATE(TSK.PRFINISH,'DD/MM/YYYY') - TO_DATE(BLD.FINISH_DATE,'DD/MM/YYYY')) > 0 THEN 1
					ELSE 0
					END TSK_ATRASADAS			  
				FROM
				  INV_INVESTMENTS INV
				  INNER JOIN INV_PROJECTS INVP ON INVP.PRID = INV.ID
				  LEFT JOIN PRTASK TSK ON TSK.PRPROJECTID = INV.ID
				  LEFT JOIN PRJ_BASELINES BL ON BL.PROJECT_ID = INV.ID AND BL.IS_CURRENT = 1
				  LEFT JOIN PRJ_BASELINE_DETAILS BLD ON BLD.OBJECT_ID = TSK.PRID AND BLD.OBJECT_TYPE = 'TASK' AND BLD.BASELINE_ID = BL.ID			
				  LEFT JOIN CMN_LOOKUPS_V STATUS ON STATUS.LOOKUP_CODE = TSK.PRSTATUS AND STATUS.LOOKUP_TYPE = 'prTaskStatus' AND STATUS.LANGUAGE_CODE = 'pt'
				WHERE
				  INV.IS_ACTIVE = 1
				  AND INVP.IS_TEMPLATE = 0
				  AND TSK.PRISTASK = 1
			  )T1
			  GROUP BY
				INV_ID,
				INV_CODE,
				INV_NAME
			)T2
		)T3

		JOIN

		(
			SELECT DISTINCT
				  INV.ID INV_ID,
				  INV.CODE INV_CODE,
				  INV.NAME INV_NAME,
				  TSK.PRID TSK_ID,
				  TSK.PRNAME TSK_NAME,
				  TSK.PRSTART TSK_START,
				  TSK.PRFINISH TSK_FINISH,
				  BLD.START_DATE BL_START,
				  BLD.FINISH_DATE BL_FINISH,
				  STATUS.NAME TSK_STATUS,
				  TP.NAME TP_NAME,
				  TP.LOOKUP_CODE TP_CODE,
				  DAS.FIRST_NAME || ' ' || DAS.LAST_NAME DIRETOR_NAME,
				  DAS.UNIQUE_NAME DIRETOR_CODE,
				  OBSU.NAME AREA_CLIENTE
				FROM
				  INV_INVESTMENTS INV
				  INNER JOIN INV_PROJECTS INVP ON INVP.PRID = INV.ID
				  LEFT JOIN PRTASK TSK ON TSK.PRPROJECTID = INV.ID
				  LEFT JOIN PRJ_BASELINES BL ON BL.PROJECT_ID = INV.ID AND BL.IS_CURRENT = 1
				  LEFT JOIN PRJ_BASELINE_DETAILS BLD ON BLD.OBJECT_ID = TSK.PRID AND BLD.OBJECT_TYPE = 'TASK' AND BLD.BASELINE_ID = BL.ID
				  INNER JOIN ODF_CA_PROJECT OPRJ ON OPRJ.ID = INV.ID
				  LEFT JOIN CMN_LOOKUPS_V TP ON TP.LOOKUP_CODE = OPRJ.obj_request_type AND TP.LOOKUP_TYPE = 'OBJ_IDEA_PROJECT_TYPE' AND TP.LANGUAGE_CODE = 'pt'
				  LEFT JOIN SRM_RESOURCES DAS ON DAS.ID = OPRJ.obj_stakeholder1
				  LEFT JOIN PRJ_OBS_ASSOCIATIONS OBSA ON OBSA.RECORD_ID = INV.ID
				  JOIN PRJ_OBS_UNITS_FLAT OBSF ON OBSA.UNIT_ID = OBSF.UNIT_ID
				  JOIN PRJ_OBS_UNITS OBSU ON OBSU.ID = OBSF.BRANCH_UNIT_ID AND OBSU.DEPTH = (
																							SELECT MAX(OBSU2.DEPTH) 
																							FROM PRJ_OBS_ASSOCIATIONS OBSA2
																							LEFT JOIN PRJ_OBS_UNITS_FLAT OBSF2 ON OBSA2.UNIT_ID = OBSF2.UNIT_ID
																							LEFT JOIN PRJ_OBS_UNITS OBSU2 ON OBSU2.ID = OBSF2.BRANCH_UNIT_ID
																							WHERE OBSA2.RECORD_ID = INV.ID 
																						)
				  JOIN PRJ_OBS_OBJECT_TYPES OOT ON OOT.TYPE_ID = OBSU.TYPE_ID AND OOT.TABLE_NAME = 'SRM_PROJECTS' AND OOT.TYPE_ID = 5000003
				  LEFT JOIN CMN_LOOKUPS_V STATUS ON STATUS.LOOKUP_CODE = TSK.PRSTATUS AND STATUS.LOOKUP_TYPE = 'prTaskStatus' AND STATUS.LANGUAGE_CODE = 'pt'
				WHERE
				  INV.IS_ACTIVE = 1
				  AND INVP.IS_TEMPLATE = 0
				  AND TSK.PRISTASK = 1
				  AND (TO_DATE(TSK.PRFINISH,'DD/MM/YYYY') - TO_DATE(BLD.FINISH_DATE,'DD/MM/YYYY')) > 0

		)TASK

		ON TASK.INV_ID = T3.INV_ID

		WHERE
			T3.DIF IS NOT NULL
	)FINAL
		
	WHERE
		$X{IN,TP_CODE,tipo_proj} 
		AND $X{IN,DIRETOR_CODE,dir_resp} 
		AND $X{IN,AREA_CLIENTE,area_resp}
)FINAL2

GROUP BY
DIF,
ORDEM]]>
		</queryString>
		<field name="DIF" class="java.lang.String"/>
		<field name="ORDEM" class="java.math.BigDecimal"/>
		<field name="QTD" class="java.math.BigDecimal"/>
		<sortField name="ORDEM"/>
	</subDataset>
	<subDataset name="DEVIO_HORAS" uuid="3bf216ad-e0cf-411f-90c2-1369fb4a5e64">
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="0"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="1000"/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="UNIMED - PRD"/>
		<property name="com.jaspersoft.studio.property.dataset.dialog.DatasetDialog.sash.w1" value="585"/>
		<property name="com.jaspersoft.studio.property.dataset.dialog.DatasetDialog.sash.w2" value="401"/>
		<queryString>
			<![CDATA[SELECT
(TSK_FINISH - BL_FINISH),
T1.*
FROM
(
  SELECT DISTINCT
    INV.ID INV_ID,
    INV.CODE INV_CODE,
    INV.NAME INV_NAME,
    MIN(TSK.PRSTART) TSK_START,
    MAX(TSK.PRFINISH) TSK_FINISH,
    MIN(BLD.START_DATE) BL_START,
    MAX(BLD.FINISH_DATE) BL_FINISH
  FROM
    INV_INVESTMENTS INV
    INNER JOIN INV_PROJECTS INVP ON INVP.PRID = INV.ID
    INNER JOIN PRTASK TSK ON TSK.PRPROJECTID = INV.ID
    INNER JOIN PRJ_BASELINES BL ON BL.PROJECT_ID = INV.ID AND BL.IS_CURRENT = 1
    INNER JOIN PRJ_BASELINE_DETAILS BLD ON BLD.OBJECT_ID = TSK.PRID AND BLD.OBJECT_TYPE = 'TASK' AND BLD.BASELINE_ID = BL.ID
  WHERE
    INV.IS_ACTIVE = 1
    AND INVP.IS_TEMPLATE = 0
    AND TSK.PRISTASK = 1  
  GROUP BY
    INV.ID,
    INV.CODE,
    INV.NAME
)T1]]>
		</queryString>
		<field name="(TSK_FINISH-BL_FINISH)" class="java.math.BigDecimal"/>
		<field name="INV_ID" class="java.math.BigDecimal"/>
		<field name="INV_CODE" class="java.lang.String"/>
		<field name="INV_NAME" class="java.lang.String"/>
		<field name="TSK_START" class="java.sql.Timestamp"/>
		<field name="TSK_FINISH" class="java.sql.Timestamp"/>
		<field name="BL_START" class="java.sql.Timestamp"/>
		<field name="BL_FINISH" class="java.sql.Timestamp"/>
	</subDataset>
	<subDataset name="TEMPO_MEDIO" uuid="7d75d029-9c4e-4d24-a274-fa803bc4d6a7">
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="0"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="1000"/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="UNIMED - PRD"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="tipo_proj" class="java.util.Collection"/>
		<parameter name="dir_resp" class="java.util.Collection"/>
		<parameter name="area_resp" class="java.util.Collection"/>
		<queryString>
			<![CDATA[SELECT
INV_ID,
INV_NAME,
DURACAO,
(
	SELECT 
		ROUND(AVG(DURACAO)) 
	FROM
	(
		SELECT (TO_DATE(OCP.UM_DT_EXEC,'DD/MM/YYYY') - TO_DATE(OCP.UM_DT_INICIO,'DD/MM/YYYY')) DURACAO			
		FROM INV_INVESTMENTS INV
		INNER JOIN INV_PROJECTS INVP ON INVP.PRID = INV.ID
		INNER JOIN ODF_CA_PROJECT OCP ON OCP.ID = INV.ID
		LEFT JOIN CMN_LOOKUPS_V TP ON TP.LOOKUP_CODE = OCP.obj_request_type AND TP.LOOKUP_TYPE = 'OBJ_IDEA_PROJECT_TYPE' AND TP.LANGUAGE_CODE = 'pt'
		LEFT JOIN SRM_RESOURCES DAS ON DAS.ID = OCP.obj_stakeholder1
		LEFT JOIN PRJ_OBS_ASSOCIATIONS OBSA ON OBSA.RECORD_ID = INV.ID
		JOIN PRJ_OBS_UNITS_FLAT OBSF ON OBSA.UNIT_ID = OBSF.UNIT_ID
		JOIN PRJ_OBS_UNITS OBSU ON OBSU.ID = OBSF.BRANCH_UNIT_ID AND OBSU.DEPTH = (
																					SELECT MAX(OBSU2.DEPTH) 
																					FROM PRJ_OBS_ASSOCIATIONS OBSA2
																					LEFT JOIN PRJ_OBS_UNITS_FLAT OBSF2 ON OBSA2.UNIT_ID = OBSF2.UNIT_ID
																					LEFT JOIN PRJ_OBS_UNITS OBSU2 ON OBSU2.ID = OBSF2.BRANCH_UNIT_ID
																					WHERE OBSA2.RECORD_ID = INV.ID 
																				)
		JOIN PRJ_OBS_OBJECT_TYPES OOT ON OOT.TYPE_ID = OBSU.TYPE_ID AND OOT.TABLE_NAME = 'SRM_PROJECTS' AND OOT.TYPE_ID = 5000003	
		WHERE INV.IS_ACTIVE = 1 AND INVP.IS_TEMPLATE = 0 AND OCP.UM_DT_EXEC IS NOT NULL AND OCP.UM_DT_INICIO IS NOT NULL
		AND $X{IN,TP_CODE,tipo_proj} AND $X{IN,DIRETOR_CODE,dir_resp} AND $X{IN,AREA_CLIENTE,area_resp} 
		AND (TO_DATE(OCP.UM_DT_EXEC,'DD/MM/YYYY') - TO_DATE(OCP.UM_DT_INICIO,'DD/MM/YYYY')) > 0
	)T2
)MEDIA
FROM
(
	SELECT DISTINCT
		INV.ID INV_ID,
		INV.CODE INV_CODE,
		INV.NAME INV_NAME,
		TP.NAME TP_NAME,
		TP.LOOKUP_CODE TP_CODE,
		DAS.FIRST_NAME || ' ' || DAS.LAST_NAME DIRETOR_NAME,
		DAS.UNIQUE_NAME DIRETOR_CODE,
		OBSU.NAME AREA_CLIENTE,
		OCP.UM_DT_INICIO DT_CRIACAO,
		OCP.UM_DT_EXEC DT_EXECUCAO,
		(TO_DATE(OCP.UM_DT_EXEC,'DD/MM/YYYY') - TO_DATE(OCP.UM_DT_INICIO,'DD/MM/YYYY')) DURACAO			
	FROM
		INV_INVESTMENTS INV
		INNER JOIN INV_PROJECTS INVP ON INVP.PRID = INV.ID
		INNER JOIN ODF_CA_PROJECT OCP ON OCP.ID = INV.ID	
		LEFT JOIN CMN_LOOKUPS_V TP ON TP.LOOKUP_CODE = OCP.obj_request_type AND TP.LOOKUP_TYPE = 'OBJ_IDEA_PROJECT_TYPE' AND TP.LANGUAGE_CODE = 'pt'
		LEFT JOIN SRM_RESOURCES DAS ON DAS.ID = OCP.obj_stakeholder1
		LEFT JOIN PRJ_OBS_ASSOCIATIONS OBSA ON OBSA.RECORD_ID = INV.ID
		JOIN PRJ_OBS_UNITS_FLAT OBSF ON OBSA.UNIT_ID = OBSF.UNIT_ID
		JOIN PRJ_OBS_UNITS OBSU ON OBSU.ID = OBSF.BRANCH_UNIT_ID AND OBSU.DEPTH = (
																					SELECT MAX(OBSU2.DEPTH) 
																					FROM PRJ_OBS_ASSOCIATIONS OBSA2
																					LEFT JOIN PRJ_OBS_UNITS_FLAT OBSF2 ON OBSA2.UNIT_ID = OBSF2.UNIT_ID
																					LEFT JOIN PRJ_OBS_UNITS OBSU2 ON OBSU2.ID = OBSF2.BRANCH_UNIT_ID
																					WHERE OBSA2.RECORD_ID = INV.ID 
																				)
		JOIN PRJ_OBS_OBJECT_TYPES OOT ON OOT.TYPE_ID = OBSU.TYPE_ID AND OOT.TABLE_NAME = 'SRM_PROJECTS' AND OOT.TYPE_ID = 5000003	
	WHERE
		INV.IS_ACTIVE = 1
		AND INVP.IS_TEMPLATE = 0
		AND OCP.UM_DT_EXEC IS NOT NULL
		AND OCP.UM_DT_INICIO IS NOT NULL
)T1
WHERE
	$X{IN,TP_CODE,tipo_proj} 
	AND $X{IN,DIRETOR_CODE,dir_resp} 
	AND $X{IN,AREA_CLIENTE,area_resp}
	AND DURACAO > 0]]>
		</queryString>
		<field name="INV_ID" class="java.math.BigDecimal"/>
		<field name="INV_NAME" class="java.lang.String"/>
		<field name="DURACAO" class="java.math.BigDecimal"/>
		<field name="MEDIA" class="java.math.BigDecimal"/>
		<sortField name="DURACAO"/>
	</subDataset>
	<subDataset name="Dataset1" uuid="59eca714-ecff-452c-80bd-c320711f0a0e">
		<queryString>
			<![CDATA[]]>
		</queryString>
	</subDataset>
	<parameter name="tipo_proj" class="java.util.Collection"/>
	<parameter name="dir_resp" class="java.util.Collection"/>
	<parameter name="area_resp" class="java.util.Collection"/>
	<queryString>
		<![CDATA[SELECT
ROUND(SUM(QTD_PROJETOS_EST),0) QTD_PROJETOS_EST,
ROUND(SUM(QTD_PROJETOS_OP),0) QTD_PROJETOS_OP,
ROUND(SUM(MEDIA),0) MEDIA
FROM
(
	SELECT 
		COUNT(INV.ID) QTD_PROJETOS_EST,
		0 QTD_PROJETOS_OP,
		0 MEDIA
	FROM 
		INV_INVESTMENTS INV
		JOIN INV_PROJECTS INVP ON INVP.PRID = INV.ID
		JOIN ODF_CA_PROJECT OCP ON OCP.ID = INV.ID
	WHERE 
		INV.IS_ACTIVE = 1 
		AND INVP.IS_TEMPLATE = 0
		AND OCP.obj_request_type = 'type100'
		
	UNION
	
	SELECT 
		0 QTD_PROJETOS_EST,
		COUNT(INV.ID) QTD_PROJETOS_OP,
		0 MEDIA
	FROM 
		INV_INVESTMENTS INV
		JOIN INV_PROJECTS INVP ON INVP.PRID = INV.ID
		JOIN ODF_CA_PROJECT OCP ON OCP.ID = INV.ID
	WHERE 
		INV.IS_ACTIVE = 1 
		AND INVP.IS_TEMPLATE = 0
		AND OCP.obj_request_type = 'type200'

	UNION

	SELECT
		0 QTD_PROJETOS_EST,
		0 QTD_PROJETOS_OP,
		AVG(DURACAO) MEDIA
	FROM
	(
		SELECT DISTINCT
			INV.ID INV_ID,
			INV.CODE INV_CODE,
			INV.NAME INV_NAME,
			OCP.UM_DT_INICIO DT_CRIACAO,
			OCP.UM_DT_EXEC DT_EXECUCAO,
			(TO_DATE(OCP.UM_DT_EXEC,'DD/MM/YYYY') - TO_DATE(OCP.UM_DT_INICIO,'DD/MM/YYYY')) DURACAO					
		FROM
			INV_INVESTMENTS INV
			INNER JOIN INV_PROJECTS INVP ON INVP.PRID = INV.ID
			INNER JOIN ODF_CA_PROJECT OCP ON OCP.ID = INV.ID			
		WHERE
			INV.IS_ACTIVE = 1
			AND INVP.IS_TEMPLATE = 0
			AND OCP.UM_DT_EXEC IS NOT NULL
			AND OCP.UM_DT_INICIO IS NOT NULL
			AND (TO_DATE(OCP.UM_DT_EXEC,'DD/MM/YYYY') - TO_DATE(OCP.UM_DT_INICIO,'DD/MM/YYYY')) >0
	)T1		
)FINAL]]>
	</queryString>
