	<subDataset name="Milestones" uuid="df8a41a6-0ddf-408c-9aca-230838499812">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="SANOFI - PROD"/>
		<parameter name="project_hide" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
  TSK.PRNAME TASK_NAME,
  TO_DATE(TSK.PRSTART,'dd/MM/yyyy') TASK_START,
  TO_DATE(TSK.PRFINISH,'dd/MM/yyyy') TASK_FINISH,
  TO_DATE(BASELINE_DETAILS.FINISH_DATE,'dd/MM/yyyy') BASELINE_FINISH,
  TO_DATE(TSK.PRFINISH,'dd/MM/yyyy') - TO_DATE(BASELINE_DETAILS.FINISH_DATE,'dd/MM/yyyy') TASK_VARIANCE, 
  CASE
    WHEN TO_DATE(TSK.PRFINISH,'dd/MM/yyyy') - TO_DATE(BASELINE_DETAILS.FINISH_DATE,'dd/MM/yyyy') <= 0 THEN 0
    ELSE 1
    END TASK_VARIANCE_STATUS,
  CASE
    WHEN TSK.PRSTATUS = 0 THEN 'Not Started'
    WHEN TSK.PRSTATUS = 1 THEN 'Started'
    ELSE 'Completed'
    END STATUS
	
FROM 
  INV_INVESTMENTS INV
  JOIN PRTASK TSK ON INV.ID = TSK.PRPROJECTID
  LEFT JOIN PRJ_BASELINES BASELINE ON BASELINE.PROJECT_ID = INV.ID
  LEFT JOIN PRJ_BASELINE_DETAILS BASELINE_DETAILS ON BASELINE_DETAILS.BASELINE_ID = BASELINE.ID AND BASELINE_DETAILS.OBJECT_ID = TSK.PRID

WHERE
  TSK.PRISMILESTONE = 1 
  AND INV.ID = $P{project_hide} 
  AND BASELINE.IS_CURRENT = 1

ORDER BY
  TSK.PRSTART ASC]]>
		</queryString>
		<field name="TASK_NAME" class="java.lang.String"/>
		<field name="TASK_START" class="java.sql.Timestamp"/>
		<field name="TASK_FINISH" class="java.sql.Timestamp"/>
		<field name="BASELINE_FINISH" class="java.sql.Timestamp"/>
		<field name="TASK_VARIANCE" class="java.math.BigDecimal"/>
		<field name="TASK_VARIANCE_STATUS" class="java.lang.Integer"/>
		<field name="STATUS" class="java.lang.String"/>
	</subDataset>
	<subDataset name="Status Report" uuid="ec2e78a3-036d-42b6-9e62-f250362b6ad6">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="SANOFI - PROD"/>
		<parameter name="project_hide" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
  STATUS_REPORT.COP_REPORT_DATE STATUS_RPT_DATE,
  STATUS_REPORT.NAME STATUS_RPT_NAME,
  STATUS_REPORT.COP_REPORT_UPDATE STATUS_RPT_UPDATE
FROM 
  INV_INVESTMENTS INV
  JOIN ODF_CA_COP_PRJ_STATUSRPT STATUS_REPORT ON STATUS_REPORT.ODF_PARENT_ID = INV.ID
WHERE
  INV.ID = $P{project_hide} 
ORDER BY
  STATUS_REPORT.COP_REPORT_DATE ASC]]>
		</queryString>
		<field name="STATUS_RPT_DATE" class="java.sql.Timestamp"/>
		<field name="STATUS_RPT_NAME" class="java.lang.String"/>
		<field name="STATUS_RPT_UPDATE" class="java.lang.String"/>
	</subDataset>
	<subDataset name="Issues in Progress" uuid="798ed209-92de-4382-b319-da7e179c3105">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="SANOFI - PRD"/>
		<parameter name="project_hide" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
	ISSUE.IMPACT_DATE ISSUE_IMPACT_DATE,
	ISSUE.TARGET_RESOLUTION_DATE ISSUE_TRD,
	ISSUE.NAME ISSUE_NAME,
	ISSUE.DESCRIPTION ISSUE_DESC,
	ISSUE.PRIORITY_CODE ISSUE_PRIORITY,
	ISSUE.RESOLUTION ISSUE_RES,
	RES.FIRST_NAME || ' ' || RES.LAST_NAME ISSUE_OWNER,
	ISSUE_STATUS.NAME ISSUE_STATUS,
	CASE
      WHEN TO_DATE(SYSDATE,'dd/MM/yyyy') - TO_DATE(ISSUE.TARGET_RESOLUTION_DATE,'dd/MM/yyyy') <= 0 THEN 0
      ELSE 1
      END ISSUE_LATE 
FROM 
	INV_INVESTMENTS INV
	JOIN RIM_RISKS_AND_ISSUES ISSUE ON ISSUE.PK_ID = INV.ID
	LEFT JOIN CMN_LOOKUPS_V ISSUE_STATUS ON ISSUE_STATUS.LOOKUP_CODE = ISSUE.STATUS_CODE AND ISSUE_STATUS.LOOKUP_TYPE = 'RIM_STATUS' AND ISSUE_STATUS.LANGUAGE_CODE = 'en'
	LEFT JOIN odf_ca_issue ODFISSUE ON ISSUE.ID = ODFISSUE.ID
	LEFT JOIN SRM_RESOURCES RES ON RES.ID = ODFISSUE.SAN_OWNER
WHERE
	  INV.ID = $P{project_hide} 
	  AND ISSUE.TYPE_CODE = 'ISSUE'
	  AND ISSUE_STATUS.NAME IN ('Open','Work in Progress')
	  AND ODFISSUE.san_visible_rpt = 1	
ORDER BY
	ISSUE.CREATED_DATE ASC]]>
		</queryString>
		<field name="ISSUE_IMPACT_DATE" class="java.sql.Timestamp"/>
		<field name="ISSUE_TRD" class="java.sql.Timestamp"/>
		<field name="ISSUE_NAME" class="java.lang.String"/>
		<field name="ISSUE_DESC" class="java.lang.String"/>
		<field name="ISSUE_PRIORITY" class="java.lang.String"/>
		<field name="ISSUE_RES" class="java.lang.String"/>
		<field name="ISSUE_OWNER" class="java.lang.String"/>
		<field name="ISSUE_STATUS" class="java.lang.String"/>
		<field name="ISSUE_LATE" class="java.lang.Integer"/>
	</subDataset>
	<subDataset name="Issues Completed" uuid="abbba763-3aa8-4cab-9899-08d3c502ac69">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="SANOFI - PRD"/>
		<parameter name="project_hide" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
	ISSUE.IMPACT_DATE ISSUE_IMPACT_DATE,
	ISSUE.TARGET_RESOLUTION_DATE ISSUE_TRD,
	ISSUE.NAME ISSUE_NAME,
	ISSUE.DESCRIPTION ISSUE_DESC,
	ISSUE.PRIORITY_CODE ISSUE_PRIORITY,
	ISSUE.RESOLUTION ISSUE_RES,
	RES.FIRST_NAME || ' ' || RES.LAST_NAME ISSUE_OWNER,
	ISSUE_STATUS.NAME ISSUE_STATUS
FROM 
	INV_INVESTMENTS INV
	JOIN RIM_RISKS_AND_ISSUES ISSUE ON ISSUE.PK_ID = INV.ID
	LEFT JOIN CMN_LOOKUPS_V ISSUE_STATUS ON ISSUE_STATUS.LOOKUP_CODE = ISSUE.STATUS_CODE AND ISSUE_STATUS.LOOKUP_TYPE = 'RIM_STATUS' AND ISSUE_STATUS.LANGUAGE_CODE = 'en'
	LEFT JOIN odf_ca_issue ODFISSUE ON ISSUE.ID = ODFISSUE.ID
	LEFT JOIN SRM_RESOURCES RES ON RES.ID = ODFISSUE.SAN_OWNER
WHERE
	INV.ID = $P{project_hide} 
	AND ISSUE.TYPE_CODE = 'ISSUE'
	AND ISSUE_STATUS.NAME IN ('Resolved')
	AND ODFISSUE.san_visible_rpt = 1	
ORDER BY
	ISSUE.CREATED_DATE ASC]]>
		</queryString>
		<field name="ISSUE_IMPACT_DATE" class="java.sql.Timestamp"/>
		<field name="ISSUE_TRD" class="java.sql.Timestamp"/>
		<field name="ISSUE_NAME" class="java.lang.String"/>
		<field name="ISSUE_DESC" class="java.lang.String"/>
		<field name="ISSUE_PRIORITY" class="java.lang.String"/>
		<field name="ISSUE_RES" class="java.lang.String"/>
		<field name="ISSUE_OWNER" class="java.lang.String"/>
		<field name="ISSUE_STATUS" class="java.lang.String"/>
	</subDataset>
	<parameter name="project_hide" class="java.lang.Integer"/>
	<parameter name="show_miles" class="java.lang.Boolean"/>
	<queryString>
		<![CDATA[SELECT
  INVI.NAME INV_NAME,
  INVI.ID INV_ID,
  INVI.CODE INV_CODE,
  BU.NAME BU,
  STATUS.NAME STATUS_INDICATOR,
  GATE.NAME STAGE_GATE,
  INVI.SCHEDULE_START START_DATE,
  INVI.SCHEDULE_FINISH FINISH_DATE,
  OCINV.SAN_PSALES PEAK_SALES,
  MAN.FIRST_NAME || ' ' || MAN.LAST_NAME MANAGER,
  BUS_OWN.FIRST_NAME || ' ' || BUS_OWN.LAST_NAME BUSINESS_OWNER,
  PROJ_TYPE.NAME PROJECT_TYPE,
  PHASE.NAME PHASE,
  FRANCHISE.NAME FRANCHISE,
  DEVELOP.NAME DEVELOP,
  OCINV.SAN_PORT_RAT PORTFOLIO_RATIONAL,
  OCINV.SAN_INDICATION_TP INDICATION_TP
  
FROM
  INV_INVESTMENTS INVI 
  JOIN ODF_CA_INV OCINV ON INVI.ID = OCINV.ID 
  JOIN ODF_CA_PROJECT OCPRJ ON INVI.ID = OCPRJ.ID 
  LEFT JOIN CMN_LOOKUPS_V BU ON BU.LOOKUP_CODE = OCINV.SAN_BU AND BU.LOOKUP_TYPE = 'SAN_LKP_BU' AND BU.LANGUAGE_CODE = 'en' 
  LEFT JOIN CMN_LOOKUPS_V STATUS ON STATUS.LOOKUP_ENUM = INVI.STATUS_INDICATOR AND STATUS.LOOKUP_TYPE = 'INVESTMENT_STATUS_INDICATOR' AND STATUS.LANGUAGE_CODE = 'en' 
  LEFT JOIN CMN_LOOKUPS_V GATE ON GATE.LOOKUP_CODE = OCINV.SAN_STAGE_GATE AND GATE.LOOKUP_TYPE = 'SAN_LKP_STAGE_GATE' AND GATE.LANGUAGE_CODE = 'en' 
  LEFT JOIN SRM_RESOURCES MAN ON MAN.USER_ID = INVI.MANAGER_ID 
  LEFT JOIN SRM_RESOURCES BUS_OWN ON BUS_OWN.ID = OCPRJ.OBJ_STAKEHOLDER1 
  LEFT JOIN CMN_LOOKUPS_V PROJ_TYPE ON PROJ_TYPE.LOOKUP_CODE = OCPRJ.OBJ_REQUEST_TYPE AND PROJ_TYPE.LOOKUP_TYPE = 'OBJ_IDEA_PROJECT_TYPE' AND PROJ_TYPE.LANGUAGE_CODE = 'en' 
  LEFT JOIN CMN_LOOKUPS_V PHASE ON INVI.STAGE_CODE = PHASE.LOOKUP_CODE AND PHASE.LOOKUP_TYPE='INV_STAGE_TYPE' AND PHASE.LANGUAGE_CODE = 'en'
	LEFT JOIN CMN_LOOKUPS_V FRANCHISE ON OCINV.SAN_FRANCHISE = FRANCHISE.LOOKUP_CODE AND FRANCHISE.LOOKUP_TYPE='SAN_LKP_FRANCHISE' AND FRANCHISE.LANGUAGE_CODE = 'en'
	LEFT JOIN CMN_LOOKUPS_V DEVELOP ON OCINV.SAN_DEVELOP = DEVELOP.LOOKUP_CODE AND DEVELOP.LOOKUP_TYPE='SAN_LKP_DEVELOPMENT' AND DEVELOP.LANGUAGE_CODE = 'en'
		
WHERE
  INVI.ID = $P{project_hide}]]>
	</queryString>
	<field name="INV_NAME" class="java.lang.String"/>
	<field name="INV_ID" class="java.math.BigDecimal"/>
	<field name="INV_CODE" class="java.lang.String"/>
