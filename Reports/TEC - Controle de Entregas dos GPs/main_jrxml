	<subDataset name="QUERYGP" uuid="1c6b9a07-d115-4773-a610-84e4e0c57cc6">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="TECBAN - TESTE"/>
		<parameter name="ger_projeto" class="java.util.Collection">
			<defaultValueExpression><![CDATA[java.util.Arrays.asList("Cinthia Regina de Oliveira","Juliano Buzzini Pulicci","Nelson Ferreira","Alexandre Marinho Dos Santos","Bruno Emilio Costa","Fabio Alexandre Franco Augusto","Gilsa Helena Pereira Gregorio","Nelson Ricardo Galiani","Otavio Aparecido Gaino Vieira","Fabio Viel dos Santos")]]></defaultValueExpression>
		</parameter>
		<parameter name="nome_projeto" class="java.util.Collection"/>
		<parameter name="raia" class="java.util.Collection">
			<parameterDescription><![CDATA[]]></parameterDescription>
		</parameter>
		<queryString>
			<![CDATA[SELECT
GERENTE_PROJETO,
COUNT(INV_ID) QUANTIDADE
FROM
  (
  SELECT
   INV.ID INV_ID
  ,INV.CODE ID_PROJETO
  ,INV.NAME NOME_PROJETO
  ,SRM.FIRST_NAME ||' ' || SRM.LAST_NAME GERENTE_PROJETO
  ,PORTF.NAME PORTFOLIO_STATUS
  ,CASE
				WHEN RAIA.NAME = 'ERP' THEN 'ERP'
				WHEN RAIA.NAME = 'Projetos de Curta Duração (Fast Track)' THEN 'Fast Track'
				WHEN RAIA.NAME = 'Projetos de Desenvolvimento de Novas Soluções de Negócios' THEN 'DNSN'
				WHEN RAIA.NAME = 'Projetos de Manutenção e Sustentação do Negócio' THEN 'MSN'
				WHEN RAIA.NAME = 'Segurança' THEN 'Segurança'
				WHEN RAIA.NAME = 'Sem CAPEX Compartilhado' THEN 'Sem CAPEX Compartilhado'
				WHEN RAIA.NAME = 'Soluções e Serviços BackOffice' THEN 'BKO'
				WHEN RAIA.NAME = 'Ferramentas Administrativas' THEN 'BKO'      
			END RAIA
  FROM INV_INVESTMENTS INV
  JOIN SRM_RESOURCES SRM ON INV.MANAGER_ID = SRM.USER_ID
  JOIN ODF_CA_PROJECT OCPRJ ON INV.ID = OCPRJ.ID
  JOIN ODF_CA_INV OCINV ON INV.ID = OCINV.ID
  JOIN INV_PROJECTS INVP ON INV.ID = INVP.PRID
  LEFT JOIN CMN_LOOKUPS_V RAIA ON RAIA.LOOKUP_CODE = OCPRJ.TB_TIPO_PRJ_PRIO_CP AND RAIA.LOOKUP_TYPE = 'TB_PC_TP_PROJETO' AND RAIA.LANGUAGE_CODE = 'pt'
  LEFT JOIN CMN_LOOKUPS_V PORTF ON PORTF.LOOKUP_CODE = OCINV.TB_PORTFT_STATUS AND PORTF.LOOKUP_TYPE = 'TB_PORTFOLIO_STATUS' AND PORTF.LANGUAGE_CODE = 'pt'
  WHERE INV.IS_ACTIVE = 1 
  AND INVP.IS_PROGRAM = 0  
  
  ) AMTS
WHERE 
$X{IN,GERENTE_PROJETO,ger_projeto}  
AND $X{IN,INV_ID,nome_projeto} 
AND $X{IN,RAIA,raia} 
AND PORTFOLIO_STATUS = 'Em Andamento'
GROUP BY GERENTE_PROJETO
ORDER BY QUANTIDADE DESC]]>
		</queryString>
		<field name="GERENTE_PROJETO" class="java.lang.String"/>
		<field name="QUANTIDADE" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="QUERYRP" uuid="36805128-cdf7-43d0-add0-5c7f280f9223">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="TECBAN - TESTE"/>
		<parameter name="ger_projeto" class="java.util.Collection"/>
		<parameter name="nome_projeto" class="java.util.Collection"/>
		<parameter name="raia" class="java.util.Collection">
			<parameterDescription><![CDATA[]]></parameterDescription>
		</parameter>
		<queryString>
			<![CDATA[SELECT
 RAIA
,COUNT(INV_ID) QUANTIDADE
FROM
(
SELECT
 INV.ID INV_ID
,INV.CODE CODIGO_PROJETO
,INV.NAME NOME_PROJETO
,SRM.FIRST_NAME ||' ' || SRM.LAST_NAME GERENTE_PROJETO
,PORTF.NAME PORTFOLIO_STATUS
,CASE
				WHEN RAIA.NAME = 'ERP' THEN 'ERP'
				WHEN RAIA.NAME = 'Projetos de Curta Duração (Fast Track)' THEN 'Fast Track'
				WHEN RAIA.NAME = 'Projetos de Desenvolvimento de Novas Soluções de Negócios' THEN 'DNSN'
				WHEN RAIA.NAME = 'Projetos de Manutenção e Sustentação do Negócio' THEN 'MSN'
				WHEN RAIA.NAME = 'Segurança' THEN 'Segurança'
				WHEN RAIA.NAME = 'Sem CAPEX Compartilhado' THEN 'Sem CAPEX Compartilhado'
				WHEN RAIA.NAME = 'Soluções e Serviços BackOffice' THEN 'BKO'
				WHEN RAIA.NAME = 'Ferramentas Administrativas' THEN 'BKO'      
			END RAIA
FROM INV_INVESTMENTS INV
JOIN ODF_CA_PROJECT OCPRJ ON INV.ID = OCPRJ.ID
JOIN SRM_RESOURCES SRM ON INV.MANAGER_ID = SRM.USER_ID
JOIN ODF_CA_INV OCINV ON INV.ID = OCINV.ID
 JOIN INV_PROJECTS INVP ON INV.ID = INVP.PRID
LEFT JOIN CMN_LOOKUPS_V RAIA ON RAIA.LOOKUP_CODE = OCPRJ.TB_TIPO_PRJ_PRIO_CP AND RAIA.LOOKUP_TYPE = 'TB_PC_TP_PROJETO' AND RAIA.LANGUAGE_CODE = 'pt'
LEFT JOIN CMN_LOOKUPS_V PORTF ON PORTF.LOOKUP_CODE = OCINV.TB_PORTFT_STATUS AND PORTF.LOOKUP_TYPE = 'TB_PORTFOLIO_STATUS' AND PORTF.LANGUAGE_CODE = 'pt'
WHERE OCPRJ.TB_TIPO_PRJ_PRIO_CP IS NOT NULL
AND INV.IS_ACTIVE = 1
AND INV.ODF_OBJECT_CODE = 'project'
AND INVP.IS_PROGRAM = 0 
) AMTS
WHERE  $X{IN,GERENTE_PROJETO,ger_projeto} 
AND $X{IN,INV_ID,nome_projeto} 
AND $X{IN,RAIA,raia} 
AND PORTFOLIO_STATUS = 'Em Andamento'
GROUP BY RAIA
ORDER BY QUANTIDADE DESC]]>
		</queryString>
		<field name="RAIA" class="java.lang.String"/>
		<field name="QUANTIDADE" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="TABLE" uuid="1af778fa-928e-4534-8606-3428cdd198f5">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="TECBAN - TESTE"/>
		<parameter name="ger_projeto" class="java.util.Collection"/>
		<parameter name="nome_projeto" class="java.util.Collection"/>
		<parameter name="raia" class="java.util.Collection">
			<parameterDescription><![CDATA[]]></parameterDescription>
		</parameter>
		<queryString>
			<![CDATA[SELECT PROJETO.INV_ID
, PROJETO.INV_CODE
, PROJETO.NOME_PROJETO
, PROJETO.GERENTE_PROJETO
, PROJETO.STATUS_REPORT_SEMANAL
, PROJETO.LINHA_BASE
, PROJETO.PROJECT_CHARTER
, PROJETO.BENEFICIOS
, PROJETO.CUSTO_PLANEJADO
, CUSTO_REAL.CUSTO_REAL
, NVL(PROJETO.CUSTO_PLANEJADO - CUSTO_REAL.CUSTO_REAL,0) DIFERENCA_PLANEJADO_REALIZADO
, PROJETO.CONTROLE_ENTREGA_CRIADO
, PROJETO.RISCO_CRIADO
, PROJETO.ALOC_ACORDADAS
, PROJETO.OCORRENCIA_GGI
, PROJETO.REALIZA_KICK_OFF
, PROJETO.CE_PLAN_VS_REAL
, PRJ_CONC.CONC_PRJ
, NVL(BL_ATUAL.BL_ATUALIZADA,'2') BL_ATUALIZADA
, REG_APROV_EXTRA.REGULARIZACAO
, OCO_STATUS_CONC.INDICADOR_STATUS_AE STATUS_CONCLUIDO_OCORRENCIA_AE
, OCO_STATUS_CONC.INDICADOR_STATUS_A STATUS_CONCLUIDO_OCORRENCIA_A
, OCO_SUBPG.STATUS_SUBPAGINA STATUS_SUBPAGINA_OCORRENCIA
, NVL(ACT_HOURS.HORAS_TRABALHADAS,0) HORAS_TRABALHADAS
, NVL(ALOCACAO.ALOC_HOURS,0) HORAS_ALOCACAO
, NVL(EPT.ETC_HOURS,0) HORAS_EPT
, CASE WHEN (PROJETO.CREATED_DATE > SYSDATE - 90) THEN '2' ELSE NVL(MORIBUNDO.VAL_HORAS_REG,0) END PROJETO_MORIBUNDO
FROM (SELECT 
  INV.ID INV_ID
, INV.CODE INV_CODE
, INV.CREATED_DATE CREATED_DATE
, INV.NAME NOME_PROJETO
, SRM.FIRST_NAME || ' ' || SRM.LAST_NAME GERENTE_PROJETO
,CASE 
WHEN EXISTS (
SELECT 1 FROM ODF_CA_CATSPRJSTATUSREP SRT 
WHERE INV.ID = SRT.ODF_PARENT_ID 
AND SRT.REPORT_DATE BETWEEN SYSDATE - 7 AND SYSDATE
) THEN 1 ELSE 0 END STATUS_REPORT_SEMANAL
,CASE 
WHEN EXISTS (
SELECT 1 FROM PRJ_BASELINES BL 
WHERE INV.ID = BL.PROJECT_ID 
) THEN 1 ELSE 0 END LINHA_BASE
,CASE 
WHEN EXISTS (
SELECT 1 FROM odf_ca_tb_obj_cont_entreg CE 
WHERE INV.ID = CE.ODF_PARENT_ID 
) THEN 1 ELSE 0 END CONTROLE_ENTREGA_CRIADO
,CASE 
WHEN EXISTS (SELECT 1 FROM ODF_CA_TB_KICK_OFF KO WHERE KO.ODF_PARENT_ID = INV.ID) THEN 1
WHEN NOT EXISTS (SELECT 1 FROM ODF_CA_TB_KICK_OFF KO WHERE KO.ODF_PARENT_ID = INV.ID) AND (SYSDATE - INV.CREATED_DATE) > 30 THEN 0
ELSE 3 END REALIZA_KICK_OFF
,CASE 
WHEN EXISTS (
SELECT 1 FROM RIM_RISKS_AND_ISSUES ISSUE JOIN ODF_CA_ISSUE OCISSUE ON ISSUE.ID = OCISSUE.ID
WHERE INV.ID = ISSUE.PK_ID 
AND OCISSUE.TB_OC_CAT_OCORR = 'tb_cat_proj_chart'
) THEN 1 ELSE 0 END PROJECT_CHARTER
,CASE 
WHEN EXISTS (
SELECT 1 FROM RIM_RISKS_AND_ISSUES RISK 
WHERE INV.ID = RISK.PK_ID AND RISK.TYPE_CODE = 'RISK'
) THEN 1 ELSE 0 END RISCO_CRIADO
,CASE 
WHEN EXISTS (
SELECT 1 FROM PRTEAM TEAM 
WHERE INV.ID = TEAM.PRPROJECTID AND TEAM.PRSTATUS !=0 AND TEAM.PRSTATUS !=4 
) THEN 1 ELSE 0 END ALOC_ACORDADAS
,CASE 
WHEN ( EXISTS (       
SELECT NULL FROM odf_ca_tb_obj_cont_entreg CE 
WHERE INV.ID = CE.ODF_PARENT_ID AND (NVL(CE.tb_cent_data_realiz,CE.tb_cent_data_plan+5)-CE.tb_cent_data_plan) > 1
)) OR (NOT EXISTS (SELECT NULL FROM odf_ca_tb_obj_cont_entreg CE 
WHERE INV.ID = CE.ODF_PARENT_ID
)) THEN 0 ELSE 1 END CE_PLAN_VS_REAL
,CASE 
WHEN EXISTS (
SELECT 1 FROM RIM_RISKS_AND_ISSUES RRI
JOIN ODF_CA_ISSUE ISSUE ON RRI.ID = ISSUE.ID
WHERE RRI.PK_ID = INV.ID
AND RRI.TYPE_CODE = 'ISSUE'
AND ISSUE.TB_OC_CAT_OCORR IN ('tb_cat_proj_aprov_extraord','tb_cat_aprov_cr','tb_cat_proj_chart','tb_cat_encer_financ','tb_cat_cr_aprov_extord')
) THEN 1 ELSE 0 END OCORRENCIA_GGI
,CASE 
WHEN EXISTS (
SELECT 1 FROM ODF_CA_TB_BENEF_PRO BENEF 
WHERE INV.ID = BENEF.ODF_PARENT_ID 
) THEN 1 ELSE 0 END BENEFICIOS
, PRJS.IS_TEMPLATE IS_TEMPLATE
, NVL(FIN.PLANNED_CST_TOTAL,0) CUSTO_PLANEJADO
,CASE
				WHEN RAIA.NAME = 'ERP' THEN 'ERP'
				WHEN RAIA.NAME = 'Projetos de Curta Duração (Fast Track)' THEN 'Fast Track'
				WHEN RAIA.NAME = 'Projetos de Desenvolvimento de Novas Soluções de Negócios' THEN 'DNSN'
				WHEN RAIA.NAME = 'Projetos de Manutenção e Sustentação do Negócio' THEN 'MSN'
				WHEN RAIA.NAME = 'Segurança' THEN 'Segurança'
				WHEN RAIA.NAME = 'Sem CAPEX Compartilhado' THEN 'Sem CAPEX Compartilhado'
				WHEN RAIA.NAME = 'Soluções e Serviços BackOffice' THEN 'BKO'
				WHEN RAIA.NAME = 'Ferramentas Administrativas' THEN 'BKO'      
			END RAIA
,PORTF.NAME PORTFOLIO_STATUS
FROM INV_INVESTMENTS INV
INNER JOIN SRM_RESOURCES SRM ON INV.MANAGER_ID = SRM.USER_ID 
INNER JOIN ODF_OBJECT_INSTANCE_MAPPING ODFF ON INV.ID = ODFF.PRIMARY_OBJECT_INSTANCE_ID AND PRIMARY_OBJECT_INSTANCE_CODE = 'project'
INNER JOIN FIN_FINANCIALS FIN ON ODFF.SECONDARY_OBJECT_INSTANCE_ID = FIN.ID AND SECONDARY_OBJECT_INSTANCE_CODE = 'financials'
INNER JOIN INV_PROJECTS PRJS ON INV.ID = PRJS.PRID
INNER JOIN ODF_CA_PROJECT OCPRJ ON INV.ID = OCPRJ.ID
INNER JOIN ODF_CA_INV OCINV ON INV.ID = OCINV.ID
LEFT JOIN CMN_LOOKUPS_V RAIA ON RAIA.LOOKUP_CODE = OCPRJ.TB_TIPO_PRJ_PRIO_CP AND RAIA.LOOKUP_TYPE = 'TB_PC_TP_PROJETO' AND RAIA.LANGUAGE_CODE = 'pt'
LEFT JOIN CMN_LOOKUPS_V PORTF ON PORTF.LOOKUP_CODE = OCINV.TB_PORTFT_STATUS AND PORTF.LOOKUP_TYPE = 'TB_PORTFOLIO_STATUS' AND PORTF.LANGUAGE_CODE = 'pt'
 

WHERE INV.ODF_OBJECT_CODE = 'project'
AND INV.IS_ACTIVE = 1
) PROJETO
LEFT JOIN
 (SELECT 
INV2.ID ID, 
NVL(SUM(PV.TOTALCOST),0) CUSTO_REAL
FROM INV_INVESTMENTS INV2
LEFT JOIN PPA_WIP PW ON INV2.ID = PW.INVESTMENT_ID
LEFT JOIN PPA_WIP_VALUES PV ON PW.TRANSNO = PV.TRANSNO 
--WHERE 
--PV.CURRENCY_TYPE = 'HOME'
--PW.TRANSDATE <= SYSDATE
--PW.STATUS = 0
GROUP BY INV2.ID, INV2.CODE, INV2.NAME) 
CUSTO_REAL ON PROJETO.INV_ID = CUSTO_REAL.ID
LEFT JOIN 
  (SELECT
    AMTS.ID ID,
    CASE WHEN ( DATA_CONC IS NOT NULL AND DATA_CONC-DATA_ENCE < 75 ) THEN '1'  
         WHEN ( DATA_ENCE IS NOT NULL AND DATA_CONC IS NULL AND SYSDATE <= DATA_ENCE +75) THEN '2'
         WHEN ( DATA_ENCE IS NULL ) THEN '2' 
         ELSE '0'  END CONC_PRJ
    FROM (SELECT
           INV3.ID ID
          ,INV3.CODE CODE
          ,INV3.NAME NAME
          ,MAX(ENCE.LAST_UPDATED_DATE) DATA_ENCE
          ,MAX(CONC.LAST_UPDATED_DATE) DATA_CONC
          FROM INV_INVESTMENTS INV3
          LEFT JOIN CMN_AUDITS ENCE ON INV3.ID = ENCE.OBJECT_ID AND ENCE.COLUMN_NAME = 'tb_portft_status' AND ENCE.RAW_VALUE_AFTER = 'tb_ps_em_encerramento'
          LEFT JOIN CMN_AUDITS CONC ON INV3.ID = CONC.OBJECT_ID AND CONC.COLUMN_NAME = 'tb_portft_status' AND CONC.RAW_VALUE_AFTER = 'tb_ps_concluido'
          GROUP BY INV3.ID, INV3.CODE, INV3.NAME) AMTS
  )PRJ_CONC ON PROJETO.INV_ID = PRJ_CONC.ID
LEFT JOIN
  (SELECT
BL_INFO.ID ID
,NVL((CASE
        WHEN ( FINISH_DATE > SYSDATE ) THEN '1' ELSE '0' END),'2') BL_ATUALIZADA
FROM(
      SELECT
        INV4.ID
      ,INV4.CODE
      ,INV4.NAME
      ,BLD.FINISH_DATE FINISH_DATE
      ,CASE
        WHEN ( FINISH_DATE > SYSDATE ) THEN '1' ELSE '0' END COMPARACAO
      FROM PRJ_BASELINES BL
      JOIN INV_INVESTMENTS INV4 ON BL.PROJECT_ID = INV4.ID
      JOIN PRJ_BASELINE_DETAILS BLD ON BL.ID = BLD.BASELINE_ID
      WHERE BLD.OBJECT_TYPE = 'PROJECT'
      AND BL.IS_CURRENT = 1) BL_INFO
    )BL_ATUAL ON PROJETO.INV_ID = BL_ATUAL.ID 
LEFT JOIN 
  (SELECT
DATAS.ID
,DATAS.CODE
,DATAS.NAME
,CASE 
  WHEN (DATA_EXTRA IS NULL) THEN '3' -- CINZA
  WHEN (DATA_EXTRA IS NOT NULL AND DATA_APROVA IS NULL AND (SYSDATE - DATA_EXTRA) <= 60) THEN '2' -- AMARELO
  WHEN (DATA_EXTRA IS NOT NULL AND DATA_APROVA IS NULL AND (SYSDATE - DATA_EXTRA) > 60) THEN '0' -- VERMELHO
  ELSE '1' -- VERDE
  END REGULARIZACAO
FROM
(
SELECT 
EXTRA.ID ID
,EXTRA.CODE
,EXTRA.NAME
,EXTRA.DATA_EXTRA
,MAX(ISSUE2.CREATED_DATE) DATA_APROVA
FROM
(SELECT 
 INV5.ID
,INV5.CODE
,INV5.NAME
,MAX(ISSUE.CREATED_DATE) DATA_EXTRA
FROM INV_INVESTMENTS INV5
LEFT JOIN RIM_RISKS_AND_ISSUES RRI ON RRI.PK_ID = INV5.ID AND RRI.TYPE_CODE = 'ISSUE'
LEFT JOIN ODF_CA_ISSUE ISSUE ON RRI.ID = ISSUE.ID AND ISSUE.tb_oc_cat_ocorr IN ('tb_cat_proj_aprov_extraord','tb_cat_cr_aprov_extord')
GROUP BY INV5.ID, INV5.CODE, INV5.NAME) EXTRA
LEFT JOIN RIM_RISKS_AND_ISSUES RRI2 ON RRI2.PK_ID = EXTRA.ID AND RRI2.TYPE_CODE = 'ISSUE' AND RRI2.CREATED_DATE > EXTRA.DATA_EXTRA
LEFT JOIN ODF_CA_ISSUE ISSUE2 ON RRI2.ID = ISSUE2.ID AND ISSUE2.tb_oc_cat_ocorr IN ('tb_cat_proj_chart','tb_cat_aprov_cr')
GROUP BY EXTRA.ID,EXTRA.CODE,EXTRA.NAME,EXTRA.DATA_EXTRA
) DATAS) REG_APROV_EXTRA ON PROJETO.INV_ID = REG_APROV_EXTRA.ID
LEFT JOIN
  (SELECT
AMTS3.ID ID
,AMTS3.CODE
,AMTS3.NAME
,MAX(STATUS_CONCLUIDO_AE) INDICADOR_STATUS_AE
,MAX(STATUS_CONCLUIDO_A) INDICADOR_STATUS_A
FROM(
SELECT
AMTS2.ID
,AMTS2.CODE
,AMTS2.NAME
,AMTS2.TIPO
, CASE
    WHEN (AMTS2.STATUS = 'tb_oc_encerrado') THEN 1 -- VERDE
    WHEN (AMTS2.TIPO = 'EXTRA' AND AMTS2.STATUS != 'tb_oc_encerrado' AND (SYSDATE - AMTS2.DATA_CRIACAO) <= 3) THEN 2 -- AMARELO
    WHEN (AMTS2.TIPO = 'EXTRA' AND AMTS2.STATUS != 'tb_oc_encerrado' AND (SYSDATE - AMTS2.DATA_CRIACAO) > 3) THEN 3 -- VERMELHO
ELSE 0 END STATUS_CONCLUIDO_AE
, CASE
    WHEN (AMTS2.STATUS = 'tb_oc_encerrado') THEN 1 -- VERDE    
    WHEN (AMTS2.TIPO = 'APROVA' AND AMTS2.STATUS != 'tb_oc_encerrado' AND (SYSDATE - AMTS2.DATA_CRIACAO) <= 15) THEN 2 -- AMARELO   
    WHEN (AMTS2.TIPO = 'APROVA' AND AMTS2.STATUS != 'tb_oc_encerrado' AND (SYSDATE - AMTS2.DATA_CRIACAO) > 15) THEN 3 -- VERMELHO
ELSE 0 END STATUS_CONCLUIDO_A
FROM(
SELECT 
 INV6.ID
,INV6.CODE
,INV6.NAME
,ISSUE6.CREATED_DATE DATA_CRIACAO
,ISSUE6.tb_oc_status_ocorr STATUS
,CASE
  WHEN (ISSUE6.tb_oc_cat_ocorr IN ('tb_cat_proj_aprov_extraord','tb_cat_cr_aprov_extord')) THEN 'EXTRA'
  ELSE 'APROVA' END TIPO
FROM INV_INVESTMENTS INV6
LEFT JOIN RIM_RISKS_AND_ISSUES RRI6 ON RRI6.PK_ID = INV6.ID AND RRI6.TYPE_CODE = 'ISSUE'
LEFT JOIN ODF_CA_ISSUE ISSUE6 ON RRI6.ID = ISSUE6.ID AND ISSUE6.tb_oc_cat_ocorr IN ('tb_cat_proj_aprov_extraord','tb_cat_cr_aprov_extord','tb_cat_proj_chart','tb_cat_aprov_cr')
) AMTS2) AMTS3
GROUP BY AMTS3.ID,AMTS3.CODE,AMTS3.NAME) OCO_STATUS_CONC ON PROJETO.INV_ID = OCO_STATUS_CONC.ID
LEFT JOIN
  (SELECT
SUBPAGINA.ID ID
,SUBPAGINA.CODE
,SUBPAGINA.NAME
,MAX(SUBPAGINA.INDICADOR) STATUS_SUBPAGINA
FROM(
SELECT 
INV7.ID ID
,INV7.CODE CODE
,INV7.NAME NAME
, CASE 
  WHEN (ISSUE7.tb_oc_cat_ocorr = 'tb_cat_proj_chart' AND ISSUE7.tb_capex_dsnt != 0 AND ISSUE7.tb_cpx_infra !=0 
        AND ISSUE7.tb_capex_moi !=0 AND ISSUE7.tb_id_rep_fin IS NOT NULL AND ISSUE7.tb_rep_fin_vlr != 0 AND ISSUE7.tb_rp_fin_pgto IS NOT NULL
        AND ISSUE7.tb_tp_ret_fin IS NOT NULL) THEN 1 --VERDE
  WHEN (ISSUE7.tb_oc_cat_ocorr = 'tb_cat_aprov_cr' AND ISSUE7.tb_capex_dsnt != 0 AND ISSUE7.tb_cpx_infra !=0 
        AND ISSUE7.tb_capex_moi !=0 AND ISSUE7.tb_id_rep_fin IS NOT NULL AND ISSUE7.tb_rep_fin_vlr != 0 AND ISSUE7.tb_rp_fin_pgto IS NOT NULL
        AND ISSUE7.tb_tp_ret_fin IS NOT NULL AND ISSUE7.tb_ft_mot_cr IS NOT NULL AND ISSUE7.tb_mot_alt_escop IS NOT NULL) THEN 1 --VERDE
  WHEN (ISSUE7.tb_oc_cat_ocorr = 'tb_cat_proj_chart' AND (ISSUE7.tb_capex_dsnt IS NULL OR ISSUE7.tb_cpx_infra IS NULL 
        OR ISSUE7.tb_capex_moi IS NULL OR ISSUE7.tb_id_rep_fin IS NULL OR ISSUE7.tb_rep_fin_vlr IS NULL OR ISSUE7.tb_rp_fin_pgto IS NULL
        OR ISSUE7.tb_tp_ret_fin IS NULL)) THEN 2 --VERMELHO
  WHEN (ISSUE7.tb_oc_cat_ocorr IS NULL) THEN 0 -- CINZA
        
        END INDICADOR
FROM INV_INVESTMENTS INV7
LEFT JOIN RIM_RISKS_AND_ISSUES RRI7 ON RRI7.PK_ID = INV7.ID AND RRI7.TYPE_CODE = 'ISSUE'
LEFT JOIN ODF_CA_ISSUE ISSUE7 ON RRI7.ID = ISSUE7.ID AND ISSUE7.tb_oc_cat_ocorr IN ('tb_cat_proj_chart','tb_cat_aprov_cr')
)SUBPAGINA
GROUP BY SUBPAGINA.ID,SUBPAGINA.CODE,SUBPAGINA.NAME) OCO_SUBPG ON PROJETO.INV_ID = OCO_SUBPG.ID
LEFT JOIN 
	(SELECT inv_investments.id ID
	,sum(t.horas_trabalhadas) horas_trabalhadas
FROM inv_investments
LEFT OUTER JOIN (
	SELECT (prteam.prprojectid * 1) prprojectid
		,(
			CASE 
				WHEN srm_resources.resource_type < 2
					THEN (
							(
								SELECT NVL(SUM(PRASSIGNMENT.PRACTSUM), 0)
								FROM PRASSIGNMENT
								WHERE PRASSIGNMENT.TEAM_ID = prteam.prid
								) / 3600
							) / (
							SELECT CASE 
									WHEN V_HPD > 0 THEN V_HPD ELSE 1 END HPD FROM (
										SELECT CASE 
												WHEN V.VALUE = 'days'
													THEN (
															SELECT C.HOURS_PER_DAY
															FROM PRCalendar c
																,PRSITE s
															WHERE c.prID = s.prCalendarID
																AND s.prCalendarID IS NOT NULL
															)
												ELSE - 1
												END V_HPD
										FROM CMN_OPTION_VALUES V
											,CMN_OPTIONS O
										WHERE V.OPTION_ID = O.ID
											AND O.IS_ACTIVE = 1
											AND O.OPTION_CODE = 'PRJ_EFFORT_DISP_UNIT'
										) t ) ELSE (
											SELECT NVL(SUM(PRASSIGNMENT.PRACTSUM), 0)
											FROM PRASSIGNMENT
											WHERE PRASSIGNMENT.TEAM_ID = prteam.prid
											) END * 1 ) horas_trabalhadas FROM prteam prteam LEFT OUTER JOIN prj_baseline_details baserec ON baserec.object_id = prteam.prid
										AND baserec.object_type = 'TEAM'
										AND baserec.is_current = 1
										,srm_resources created
										,srm_resources updated
										,srm_resources srm_resources LEFT OUTER JOIN srm_resources res_manager ON srm_resources.manager_id = res_manager.user_id
										,prj_resources prj_resources
										,inv_investments inv_investments LEFT OUTER JOIN inv_projects inv_projects ON inv_investments.id = inv_projects.prid
										,odf_ca_team odf_ca_team
										,(
											SELECT 'x' pmd_analytical_partition_by
												,inv_investments.id odf_pk
												,odf_ca_inv.odf_period_end odf_period_end
												,odf_ca_inv.odf_period_type odf_period_type
												,odf_ca_inv.odf_period_start odf_period_start
												,inv_investments.entity_code entity
											FROM inv_investments inv_investments
												,odf_ca_inv odf_ca_inv
											WHERE 1 = 1
												AND inv_investments.id = odf_ca_inv.id
											) prprojectid WHERE 1 = 1
										AND prteam.created_by = created.user_id
										AND prteam.last_updated_by = updated.user_id
										AND prteam.prresourceid = srm_resources.id
										AND prteam.prresourceid = prj_resources.prid
										AND prteam.prprojectid = inv_investments.id
										AND prteam.prid = odf_ca_team.id
										AND prteam.prprojectid = prprojectid.odf_pk
										AND 1 = 1
										AND 1 = 1 ) t ON inv_investments.id = t.prprojectid 
                                        GROUP BY inv_investments.id) ACT_HOURS ON PROJETO.INV_ID = ACT_HOURS.ID
LEFT JOIN 
  (SELECT 
INV.ID ID
,INV.CODE CODE
,INV.NAME NAME
,SUM(TM.PRALLOCSUM/3600) ALOC_HOURS
FROM INV_INVESTMENTS INV
JOIN PRTEAM TM ON INV.ID = TM.PRPROJECTID
JOIN SRM_RESOURCES SRM ON TM.PRRESOURCEID = SRM.ID
JOIN PRJ_RESOURCES PRJR ON SRM.ID = PRJR.PRID
AND inv.odf_object_code = 'project' 
GROUP BY INV.ID, INV.CODE, INV.NAME) ALOCACAO ON PROJETO.INV_ID = ALOCACAO.ID

LEFT JOIN 
  (SELECT 
INV.ID ID
,INV.CODE CODE
,INV.NAME NAME
,SUM(PASS.PRESTSUM/3600) ETC_HOURS
FROM INV_INVESTMENTS INV
JOIN PRTEAM TM ON INV.ID = TM.PRPROJECTID
JOIN SRM_RESOURCES SRM ON TM.PRRESOURCEID = SRM.ID
JOIN PRJ_RESOURCES PRJR ON SRM.ID = PRJR.PRID
JOIN PRASSIGNMENT PASS ON TM.PRID = PASS.TEAM_ID
AND INV.odf_object_code = 'project' 
GROUP BY INV.ID, INV.CODE, INV.NAME) EPT ON PROJETO.INV_ID = EPT.ID

LEFT JOIN
  (SELECT
APONTAMENTO.ID ID
,CASE 
  WHEN (APONTAMENTO.HORAS_REGISTRADAS >= 100) THEN '1' ELSE '0' END VAL_HORAS_REG
FROM( 
SELECT
INV.ID
,INV.CODE
,INV.NAME
, NVL(sum((case when TS.prstatus = 4 then slices.slice else 0 end)),1) HORAS_REGISTRADAS
, SLICES.SLICE_DATE
FROM PRTIMESHEET TS
JOIN PRTIMEENTRY TE ON TS.PRID = TE.PRTIMESHEETID 
JOIN PRASSIGNMENT PASS ON TE.PRASSIGNMENTID = PASS.PRID 
JOIN PRTEAM TM ON PASS.TEAM_ID = TM.PRID
JOIN INV_INVESTMENTS INV ON TM.PRPROJECTID = INV.ID
JOIN prj_blb_slices slices ON TE.prid = slices.prj_object_id
AND SLICES.SLICE_DATE > SYSDATE - 90
JOIN prj_blb_slicerequests slice_req ON slices.slice_request_id = slice_req.id 
AND slice_req.request_name = 'MES_RES_TIME_CURVE' -- 'DAILYRESOURCETIMECURVE'
WHERE INV.ODF_OBJECT_CODE = 'project'
AND TS.prstatus = '4'
AND TS.prid not in (select pradjustedid from prtimesheet where prisadjustment = 1)
GROUP BY INV.ID,INV.CODE,INV.NAME, SLICES.SLICE_DATE
) APONTAMENTO) MORIBUNDO ON PROJETO.INV_ID = MORIBUNDO.ID
WHERE PROJETO.IS_TEMPLATE = 0
AND $X{IN,GERENTE_PROJETO,ger_projeto}  
AND $X{IN,INV_ID,nome_projeto}
AND $X{IN,RAIA,raia} 
AND PORTFOLIO_STATUS = 'Em Andamento']]>
		</queryString>
		<field name="INV_ID" class="java.math.BigDecimal"/>
		<field name="INV_CODE" class="java.lang.String"/>
		<field name="NOME_PROJETO" class="java.lang.String"/>
		<field name="GERENTE_PROJETO" class="java.lang.String"/>
		<field name="STATUS_REPORT_SEMANAL" class="java.lang.Integer"/>
		<field name="LINHA_BASE" class="java.lang.Integer"/>
