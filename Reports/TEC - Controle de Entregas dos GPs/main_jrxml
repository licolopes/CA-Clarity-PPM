		<field name="STATUS_CONCLUIDO_OCORRENCIA_AE" class="java.lang.Integer"/>
		<field name="STATUS_CONCLUIDO_OCORRENCIA_A" class="java.lang.Integer"/>
		<field name="STATUS_SUBPAGINA_OCORRENCIA" class="java.lang.Integer"/>
		<field name="HORAS_TRABALHADAS" class="java.math.BigDecimal"/>
		<field name="HORAS_ALOCACAO" class="java.math.BigDecimal"/>
		<field name="HORAS_EPT" class="java.math.BigDecimal"/>
		<field name="PROJETO_MORIBUNDO" class="java.lang.Integer"/>
	</subDataset>
	<parameter name="ger_projeto" class="java.util.Collection">
		<defaultValueExpression><![CDATA[java.util.Arrays.asList("Cinthia Regina de Oliveira","Juliano Buzzini Pulicci","Nelson Ferreira","Alexandre Marinho Dos Santos","Bruno Emilio Costa","Fabio Alexandre Franco Augusto","Gilsa Helena Pereira Gregorio","Nelson Ricardo Galiani","Otavio Aparecido Gaino Vieira","Fabio Viel dos Santos")]]></defaultValueExpression>
