	<subDataset name="tasks" uuid="f27e68a1-a92f-4aeb-b209-c8dbb7c52ed9">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="TECBAN - TEST"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="project_hide" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
  PRID,
  PRNAME,
  PRSTART,
  (PRFINISH+60) PRFINISH,
  PRPCTCOMPLETE
FROM 
  PRTASK
WHERE 
  PRPROJECTID = $P{project_hide} 
  AND PRISMILESTONE = 1
ORDER BY
  PRSTART ASC]]>
		</queryString>
		<field name="PRID" class="java.math.BigDecimal"/>
		<field name="PRNAME" class="java.lang.String"/>
		<field name="PRSTART" class="java.sql.Timestamp"/>
		<field name="PRFINISH" class="java.sql.Timestamp"/>
		<field name="PRPCTCOMPLETE" class="java.lang.Double"/>
	</subDataset>
	<subDataset name="benefit" uuid="0c7f5859-152f-4bf8-85b3-6023b37b573e">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="SANOFI - DEV"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="INV_ID" class="java.lang.String"/>
		<parameter name="project_hide" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT 
INV_ID
, SUM(Y1) Y1
, SUM(Y2) Y2
, SUM(Y3) Y3
, SUM(Y4) Y4
, SUM(Y5) Y5
, CLASS
, SUBCLASS

FROM(

SELECT
INV_ID
, CASE WHEN YEAR = INV_YEAR THEN SUM(TOTAL) ELSE 0 END Y1 
, CASE WHEN YEAR = INV_YEAR+1 THEN SUM(TOTAL) ELSE 0 END Y2 
, CASE WHEN YEAR = INV_YEAR+2 THEN SUM(TOTAL) ELSE 0 END Y3 
, CASE WHEN YEAR = INV_YEAR+3 THEN SUM(TOTAL) ELSE 0 END Y4 
, CASE WHEN YEAR = INV_YEAR+4 THEN SUM(TOTAL) ELSE 0 END Y5 
, CLASS
, SUBCLASS
FROM
(
SELECT P.OBJECT_ID INV_ID, 
bpd_ssl.slice * (bpd_ssl.finish_date - bpd_ssl.start_date) total, TO_CHAR(bpd_ssl.start_date,'YYYY') YEAR, TO_CHAR(INVI.SCHEDULE_FINISH,'YYYY') INV_YEAR, BEN_CLASS.NAME CLASS, BEN_SUBCLASS.NAME SUBCLASS
    FROM FIN_PLANS p
    INNER JOIN FIN_PLANS bp on p.benefit_plan_id = bp.id
    INNER JOIN FIN_BENEFIT_PLAN_DETAILS bpd on bp.id = bpd.plan_id
    INNER JOIN odf_ca_benefitplandetail obp on bpd.id = obp.id
    LEFT JOIN odf_ssl_bft_dtl_bft bpd_ssl ON bpd_ssl.prj_object_id = bpd.id
    LEFT JOIN CMN_LOOKUPS_V BEN_CLASS ON BEN_CLASS.LOOKUP_CODE = obp.obj_benefit_class AND BEN_CLASS.LANGUAGE_CODE = 'en'  AND BEN_CLASS.LOOKUP_TYPE = 'OBJ_BENEFITDETAIL_CLASS'
    LEFT JOIN CMN_LOOKUPS_V BEN_SUBCLASS ON BEN_SUBCLASS.LOOKUP_CODE = obp.obj_benefit_subclass AND BEN_SUBCLASS.LANGUAGE_CODE = 'en' AND BEN_SUBCLASS.LOOKUP_TYPE = 'OBJ_BENEFITDETAIL_SUBCLASS'
    JOIN INV_INVESTMENTS INVI ON P.OBJECT_ID = INVI.ID
    WHERE p.is_plan_of_record = 1
    AND p.plan_type_code = 'FORECAST'
	AND bpd_ssl.slice IS NOT NULL
    AND p.OBJECT_ID =  $P{project_hide} 
)
GROUP BY
INV_ID
, CLASS
, SUBCLASS
, YEAR
, INV_YEAR
)

GROUP BY
INV_ID
, CLASS
, SUBCLASS

ORDER BY CLASS, SUBCLASS]]>
		</queryString>
		<field name="INV_ID" class="java.math.BigDecimal"/>
		<field name="Y1" class="java.math.BigDecimal"/>
		<field name="Y2" class="java.math.BigDecimal"/>
		<field name="Y3" class="java.math.BigDecimal"/>
		<field name="Y4" class="java.math.BigDecimal"/>
		<field name="Y5" class="java.math.BigDecimal"/>
		<field name="CLASS" class="java.lang.String"/>
		<field name="SUBCLASS" class="java.lang.String"/>
	</subDataset>
	<subDataset name="stakeholders" uuid="af98e9b5-7a8c-4e0c-b991-00078d90c769">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="SANOFI - PRD"/>
		<parameter name="project_hide" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
AREA,
LISTAGG(RES, ', ') WITHIN GROUP (ORDER BY RES) RES
FROM
(
  SELECT
  INV.ID INV_ID,
  1 AREA_SORT,
  'Project Manager' AREA,
  RES.FIRST_NAME || ' ' || RES.LAST_NAME RES
  FROM 
    INV_INVESTMENTS INV
    LEFT JOIN SRM_RESOURCES RES ON RES.USER_ID = INV.MANAGER_ID
  
  UNION ALL
  
  SELECT
  INV.ID INV_ID,
  2 AREA_SORT,
  'CSU' AREA,
  RES.FIRST_NAME || ' ' || RES.LAST_NAME RES
  FROM 
    INV_INVESTMENTS INV
    LEFT JOIN ODF_MULTI_VALUED_LOOKUPS MUL ON INV.ID = MUL.PK_ID AND MUL.OBJECT = 'project' AND MUL.ATTRIBUTE = 'san_assigned_csu'
    LEFT JOIN SRM_RESOURCES RES ON RES.ID = MUL.VALUE
  WHERE
    MUL.VALUE IS NOT NULL
	
  UNION ALL
  
  SELECT
  INV.ID INV_ID,
  3 AREA_SORT,
  'Finance' AREA,
  RES.FIRST_NAME || ' ' || RES.LAST_NAME RES
  FROM 
    INV_INVESTMENTS INV
    LEFT JOIN ODF_MULTI_VALUED_LOOKUPS MUL ON INV.ID = MUL.PK_ID AND MUL.OBJECT = 'project' AND MUL.ATTRIBUTE = 'san_assigned_finance'
    LEFT JOIN SRM_RESOURCES RES ON RES.ID = MUL.VALUE
  WHERE
    MUL.VALUE IS NOT NULL
  
  UNION ALL
  
  SELECT
  INV.ID INV_ID,
  4 AREA_SORT,
  'IDI' AREA,
  RES.FIRST_NAME || ' ' || RES.LAST_NAME RES
  FROM 
    INV_INVESTMENTS INV
    LEFT JOIN ODF_MULTI_VALUED_LOOKUPS MUL ON INV.ID = MUL.PK_ID AND MUL.OBJECT = 'project' AND MUL.ATTRIBUTE = 'san_assigned_idi'
    LEFT JOIN SRM_RESOURCES RES ON RES.ID = MUL.VALUE
  WHERE
    MUL.VALUE IS NOT NULL
	
  UNION ALL
  
  SELECT
  INV.ID INV_ID,
  5 AREA_SORT,
  'Innovation' AREA,
  RES.FIRST_NAME || ' ' || RES.LAST_NAME RES
  FROM 
    INV_INVESTMENTS INV
    LEFT JOIN ODF_MULTI_VALUED_LOOKUPS MUL ON INV.ID = MUL.PK_ID AND MUL.OBJECT = 'project' AND MUL.ATTRIBUTE = 'san_assigned_inn'
    LEFT JOIN SRM_RESOURCES RES ON RES.ID = MUL.VALUE
  WHERE
    MUL.VALUE IS NOT NULL
	
  UNION ALL
  
  SELECT
  INV.ID INV_ID,
  6 AREA_SORT,
  'Legal and Trademark' AREA,
  RES.FIRST_NAME || ' ' || RES.LAST_NAME RES
  FROM 
    INV_INVESTMENTS INV
    LEFT JOIN ODF_MULTI_VALUED_LOOKUPS MUL ON INV.ID = MUL.PK_ID AND MUL.OBJECT = 'project' AND MUL.ATTRIBUTE = 'san_assigned_legal'
    LEFT JOIN SRM_RESOURCES RES ON RES.ID = MUL.VALUE
  WHERE
    MUL.VALUE IS NOT NULL
	
  UNION ALL
  
  SELECT
  INV.ID INV_ID,
  7 AREA_SORT,
  'Medical' AREA,
  RES.FIRST_NAME || ' ' || RES.LAST_NAME RES
  FROM 
    INV_INVESTMENTS INV
    LEFT JOIN ODF_MULTI_VALUED_LOOKUPS MUL ON INV.ID = MUL.PK_ID AND MUL.OBJECT = 'project' AND MUL.ATTRIBUTE = 'san_assigned_medical'
    LEFT JOIN SRM_RESOURCES RES ON RES.ID = MUL.VALUE
  WHERE
    MUL.VALUE IS NOT NULL
	
  UNION ALL
  
  SELECT
  INV.ID INV_ID,
  8 AREA_SORT,
  'Patents' AREA,
  RES.FIRST_NAME || ' ' || RES.LAST_NAME RES
  FROM 
    INV_INVESTMENTS INV
    LEFT JOIN ODF_MULTI_VALUED_LOOKUPS MUL ON INV.ID = MUL.PK_ID AND MUL.OBJECT = 'project' AND MUL.ATTRIBUTE = 'san_assigned_patents'
    LEFT JOIN SRM_RESOURCES RES ON RES.ID = MUL.VALUE
  WHERE
    MUL.VALUE IS NOT NULL
	
  UNION ALL
  
  SELECT
  INV.ID INV_ID,
  9 AREA_SORT,
  'Port. Licensing' AREA,
  RES.FIRST_NAME || ' ' || RES.LAST_NAME RES
  FROM 
    INV_INVESTMENTS INV
    LEFT JOIN ODF_MULTI_VALUED_LOOKUPS MUL ON INV.ID = MUL.PK_ID AND MUL.OBJECT = 'project' AND MUL.ATTRIBUTE = 'san_assigned_prt_lic'
    LEFT JOIN SRM_RESOURCES RES ON RES.ID = MUL.VALUE
  WHERE
    MUL.VALUE IS NOT NULL
	
  UNION ALL
  
  SELECT
  INV.ID INV_ID,
  10 AREA_SORT,
  'Pricing' AREA,
  RES.FIRST_NAME || ' ' || RES.LAST_NAME RES
  FROM 
    INV_INVESTMENTS INV
    LEFT JOIN ODF_MULTI_VALUED_LOOKUPS MUL ON INV.ID = MUL.PK_ID AND MUL.OBJECT = 'project' AND MUL.ATTRIBUTE = 'san_assigned_pricing'
    LEFT JOIN SRM_RESOURCES RES ON RES.ID = MUL.VALUE
  WHERE
    MUL.VALUE IS NOT NULL
	
  UNION ALL
  
  SELECT
  INV.ID INV_ID,
  11 AREA_SORT,
  'Regulatory' AREA,
  RES.FIRST_NAME || ' ' || RES.LAST_NAME RES
  FROM 
    INV_INVESTMENTS INV
    LEFT JOIN ODF_MULTI_VALUED_LOOKUPS MUL ON INV.ID = MUL.PK_ID AND MUL.OBJECT = 'project' AND MUL.ATTRIBUTE = 'san_assigned_reg'
    LEFT JOIN SRM_RESOURCES RES ON RES.ID = MUL.VALUE
)T1

WHERE
  INV_ID = $P{project_hide} 

GROUP BY
  AREA,
  AREA_SORT

ORDER BY
  AREA_SORT ASC]]>
		</queryString>
		<field name="AREA" class="java.lang.String"/>
		<field name="RES" class="java.lang.String"/>
	</subDataset>
	<parameter name="project_hide" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
  INVI.ID INV_ID,
  INVI.NAME INV_NAME,
  INVI.CODE INV_CODE,
  SYSDATE CURR_DATE,
  STR_REV.NAME STR_REV,
  OCINV.SAN_MED_EVA MED_EVAL,
  OCINV.SAN_PROD_HIST_BACKG HIS_BACK,
  OCINV.SAN_INDICATION INDICATION,
  OCINV.SAN_BRAND_NAME ORIGINATOR,
  (
    SELECT
      LISTAGG(SFD.STRENGTH || ' x ' || SFD.SU_PACK, ' | ') WITHIN GROUP (ORDER BY SFD.STRENGTH,SFD.SU_PACK)
    FROM 
      ODF_CA_SAN_SALES_FCST SF
      JOIN ODF_CA_SAN_SALES_FCST_DET SFD ON SFD.ODF_PARENT_ID = SF.ID
    WHERE
      SF.ODF_PARENT_ID = INVI.ID
      AND SF.PLAN_OF_RECORD = 1
  )SKU,
  OCINV.SAN_BRAND_NAME BRAND_NAME,
  OCINV.SAN_COM_DIS_RAT PRICE_STRATEGY,
  OCINV.SAN_COM_LM PROPOSED_SOURCE,
  INVI.SCHEDULE_FINISH LAUNCH,
  (
    SELECT FIN.SAN_PB_LAUNCH_NDISC
    FROM ODF_CA_SAN_FINANCE FIN
    WHERE FIN.ODF_PARENT_ID = INVI.ID AND FIN.IS_ACTIVE = 1
  )PAYBACK_ND,
  (
    SELECT ROUND(FIN.SAN_NPV_WOTV,2)
    FROM ODF_CA_SAN_FINANCE FIN
    WHERE FIN.ODF_PARENT_ID = INVI.ID AND FIN.IS_ACTIVE = 1
  )NPV_WO,
  OCINV.DEV_IDI_INV INV_DEV,
  OCINV.SAN_REGULATORY REGULATORY,
  OCINV.SAN_RA_STRATEGY RA_STRATEGY,
  IDI_REC.NAME IDI_REC,
  OCINV.SAN_COMMENTS_DV IDI_REC_COM  

 FROM 
  INV_INVESTMENTS INVI
  INNER JOIN ODF_CA_INV OCINV ON INVI.ID = OCINV.ID
  INNER JOIN CMN_LOOKUPS_V STR_REV ON STR_REV.LOOKUP_ENUM = OCINV.SAN_PRI_STRG_RELEV2 AND STR_REV.LANGUAGE_CODE = 'en' AND STR_REV.LOOKUP_TYPE = 'SAN_LKP_STRG_RELEV2'
  LEFT JOIN CMN_LOOKUPS_V IDI_REC ON IDI_REC.ID = OCINV.SAN_REC_DEVELOP AND IDI_REC.LANGUAGE_CODE = 'en' AND IDI_REC.LOOKUP_TYPE = 'PAC_RPT_YESNO'
 WHERE 
  INVI.ID = $P{project_hide}]]>
	</queryString>
	<field name="INV_ID" class="java.math.BigDecimal"/>
	<field name="INV_NAME" class="java.lang.String"/>
	<field name="INV_CODE" class="java.lang.String"/>
	<field name="CURR_DATE" class="java.sql.Timestamp"/>
	<field name="STR_REV" class="java.lang.String"/>
	<field name="MED_EVAL" class="java.lang.String"/>
	<field name="HIS_BACK" class="java.lang.String"/>
	<field name="INDICATION" class="java.lang.String"/>
	<field name="ORIGINATOR" class="java.lang.String"/>
	<field name="SKU" class="java.lang.String"/>
	<field name="BRAND_NAME" class="java.lang.String"/>
	<field name="PRICE_STRATEGY" class="java.lang.String"/>
	<field name="PROPOSED_SOURCE" class="java.lang.String"/>
	<field name="LAUNCH" class="java.sql.Timestamp"/>
	<field name="PAYBACK_ND" class="java.lang.String"/>
	<field name="NPV_WO" class="java.math.BigDecimal"/>
	<field name="INV_DEV" class="java.math.BigDecimal"/>
	<field name="REGULATORY" class="java.lang.String"/>
	<field name="RA_STRATEGY" class="java.lang.String"/>
	<field name="IDI_REC" class="java.lang.String"/>
	<field name="IDI_REC_COM" class="java.lang.String"/>
