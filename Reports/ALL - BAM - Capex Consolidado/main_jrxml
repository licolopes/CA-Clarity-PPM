	<subDataset name="CAPEX_DEMANDA_PROJ" uuid="004a4746-3085-4742-9ad2-8402ee307845">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="RUMO - PRD"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="0"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="1000"/>
		<queryString>
			<![CDATA[SELECT
 TIPO_DEMANDA || ' R$' TIPO_DEMANDA
, (SUM(NVL(CORPORATIVO,0)))/1000 CORPORATIVO
, (SUM(NVL(NORTE,0)))/1000 NORTE
, (SUM(NVL(SUL,0)))/1000 SUL
, 'VAL' TIPO
, CASE WHEN TIPO_DEMANDA = 'CAPEX' THEN 1 ELSE 10 END ORDEM
FROM
(
	SELECT DISTINCT
		INVI.CODE INV_CODE
		, TIPO_DEMANDA.NAME TIPO_DEMANDA 
		, CASE WHEN OPERACAO.NAME = 'Corporativo' THEN NVL(ROUND(OCP.all_cst_total_fct,0),0) END CORPORATIVO
		, CASE WHEN OPERACAO.NAME = 'Operação Norte' THEN NVL(ROUND(OCP.all_cst_total_fct,0),0) END NORTE
		, CASE WHEN OPERACAO.NAME = 'Operação Sul' THEN NVL(ROUND(OCP.all_cst_total_fct,0),0) END SUL
	FROM 
		INV_INVESTMENTS INVI
		LEFT JOIN ODF_CA_INV OCINV ON INVI.ID = OCINV.ID
		LEFT JOIN ODF_CA_PROJECT OCPRJ ON OCPRJ.ID = INVI.ID
		LEFT JOIN ODF_CA_IDEA OCID ON OCID.ID = INVI.ID AND INVI.STATUS !=8
		LEFT JOIN CMN_LOOKUPS_V TIPO_DEMANDA ON (TIPO_DEMANDA.LOOKUP_CODE = OCPRJ.all_tipo_demanda OR TIPO_DEMANDA.LOOKUP_CODE = OCID.all_tipo_demanda) AND TIPO_DEMANDA.LANGUAGE_CODE = 'pt' AND TIPO_DEMANDA.LOOKUP_TYPE = 'ALL_TIPO_DEMANDA'
		LEFT JOIN CMN_LOOKUPS_V OPERACAO ON OPERACAO.LOOKUP_CODE = OCINV.all_area AND OPERACAO.LANGUAGE_CODE = 'pt' AND OPERACAO.LOOKUP_TYPE = 'ALL_LKP_AREAS'
		LEFT JOIN FIN_PLANS FP ON FP.OBJECT_ID = INVI.ID AND FP.PLAN_TYPE_CODE = 'FORECAST'
		LEFT JOIN FIN_COST_PLAN_DETAILS FPD on FP.ID = FPD.PLAN_ID
		LEFT JOIN ODF_SSL_CST_DTL_COST FP_COST ON FPD.ID = FP_COST.PRJ_OBJECT_ID
		LEFT JOIN ODF_CA_COSTPLAN OCP ON OCP.ID = FP.ID AND OCP.ALL_TP_PLANO = 'all_forecast' AND OCP.ALL_ATIVO = 1
	WHERE 
		TIPO_DEMANDA.LOOKUP_CODE IS NOT NULL
		AND INVI.STATUS !=8
		AND INVI.IS_ACTIVE = 1
		AND (
				(TRUNC(FP_COST.START_DATE,'YEAR') = TRUNC(SYSDATE,'YEAR'))
				OR
				(TRUNC(SYSDATE,'YEAR') = 
				(	
					SELECT DISTINCT 
					TRUNC(PW.TRANSDATE,'YEAR') 
					FROM PPA_WIP PW 
					LEFT JOIN PPA_WIP_VALUES PV ON PW.TRANSNO = PV.TRANSNO AND PV.CURRENCY_TYPE = 'HOME'
					WHERE PW.INVESTMENT_ID = INVI.ID AND PW.STATUS = 0 AND PV.TOTALCOST > 0 AND TRUNC(PW.TRANSDATE,'YEAR') = TRUNC(SYSDATE,'YEAR')
				))
			)
) VAL

GROUP BY 
TIPO_DEMANDA

UNION

SELECT
 'Quantidade ' TIPO_DEMANDA
, SUM(NVL(CORPORATIVO,0)) CORPORATIVO
, SUM(NVL(NORTE,0)) NORTE
, SUM(NVL(SUL,0)) SUL
, 'QTD' TIPO
, CASE WHEN TIPO_DEMANDA = 'CAPEX' THEN 2 ELSE 20 END ORDEM
FROM
(
	SELECT DISTINCT
		INVI.CODE INV_CODE
		, TIPO_DEMANDA.NAME TIPO_DEMANDA 
		, CASE WHEN OPERACAO.NAME = 'Corporativo' THEN 1 END CORPORATIVO
		, CASE WHEN OPERACAO.NAME = 'Operação Norte' THEN 1 END NORTE
		, CASE WHEN OPERACAO.NAME = 'Operação Sul' THEN 1 END SUL
	FROM 
		INV_INVESTMENTS INVI
		LEFT JOIN ODF_CA_INV OCINV ON INVI.ID = OCINV.ID
		LEFT JOIN ODF_CA_PROJECT OCPRJ ON OCPRJ.ID = INVI.ID
		LEFT JOIN ODF_CA_IDEA OCID ON OCID.ID = INVI.ID
		LEFT JOIN CMN_LOOKUPS_V TIPO_DEMANDA ON (TIPO_DEMANDA.LOOKUP_CODE = OCPRJ.all_tipo_demanda OR TIPO_DEMANDA.LOOKUP_CODE = OCID.all_tipo_demanda) AND TIPO_DEMANDA.LANGUAGE_CODE = 'pt' AND TIPO_DEMANDA.LOOKUP_TYPE = 'ALL_TIPO_DEMANDA'
		LEFT JOIN CMN_LOOKUPS_V OPERACAO ON OPERACAO.LOOKUP_CODE = OCINV.all_area AND OPERACAO.LANGUAGE_CODE = 'pt' AND OPERACAO.LOOKUP_TYPE = 'ALL_LKP_AREAS'
		LEFT JOIN FIN_PLANS FP ON FP.OBJECT_ID = INVI.ID AND FP.PLAN_TYPE_CODE = 'FORECAST'
		LEFT JOIN FIN_COST_PLAN_DETAILS FPD on FP.ID = FPD.PLAN_ID
		LEFT JOIN ODF_SSL_CST_DTL_COST FP_COST ON FPD.ID = FP_COST.PRJ_OBJECT_ID
		LEFT JOIN ODF_CA_COSTPLAN OCP ON OCP.ID = FP.ID AND OCP.ALL_TP_PLANO = 'all_forecast' AND OCP.ALL_ATIVO = 1
	WHERE 
		TIPO_DEMANDA.LOOKUP_CODE IS NOT NULL
		AND INVI.STATUS !=8
		AND OCP.all_cst_total_fct <> 0
		AND INVI.IS_ACTIVE = 1
		AND (
				(TRUNC(FP_COST.START_DATE,'YEAR') = TRUNC(SYSDATE,'YEAR'))
				OR
				(TRUNC(SYSDATE,'YEAR') = 
				(	
					SELECT DISTINCT 
					TRUNC(PW.TRANSDATE,'YEAR') 
					FROM PPA_WIP PW 
					LEFT JOIN PPA_WIP_VALUES PV ON PW.TRANSNO = PV.TRANSNO AND PV.CURRENCY_TYPE = 'HOME'
					WHERE PW.INVESTMENT_ID = INVI.ID AND PW.STATUS = 0 AND PV.TOTALCOST > 0 AND TRUNC(PW.TRANSDATE,'YEAR') = TRUNC(SYSDATE,'YEAR')
				))
			)
) VAL

GROUP BY 
TIPO_DEMANDA]]>
		</queryString>
		<field name="TIPO_DEMANDA" class="java.lang.String"/>
		<field name="CORPORATIVO" class="java.math.BigDecimal"/>
		<field name="NORTE" class="java.math.BigDecimal"/>
		<field name="SUL" class="java.math.BigDecimal"/>
		<field name="TIPO" class="java.lang.String"/>
		<field name="ORDEM" class="java.math.BigDecimal"/>
		<sortField name="ORDEM"/>
		<variable name="TOTAL_LINHA" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$F{NORTE}.add($F{SUL}).add($F{CORPORATIVO})]]></variableExpression>
		</variable>
		<variable name="TOTAL_CORP_VAL" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{TIPO}.equals("VAL") ? $F{CORPORATIVO} : 0.0]]></variableExpression>
		</variable>
		<variable name="TOTAL_NORTE_VAL" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{TIPO}.equals("VAL") ? $F{NORTE} : 0]]></variableExpression>
		</variable>
		<variable name="TOTAL_SUL_VAL" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{TIPO}.equals("VAL")?$F{SUL}:0]]></variableExpression>
		</variable>
		<variable name="TOTAL_CORP_QTD" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{TIPO}.equals("QTD") ? $F{CORPORATIVO} : 0.0]]></variableExpression>
		</variable>
		<variable name="TOTAL_NORTE_QTD" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{TIPO}.equals("QTD") ? $F{NORTE} : 0.0]]></variableExpression>
		</variable>
		<variable name="TOTAL_SUL_QTD" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{TIPO}.equals("QTD") ? $F{SUL} : 0.0]]></variableExpression>
		</variable>
		<variable name="TOTAL_TOTAL_VAL" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{TIPO}.equals("VAL") ? $V{TOTAL_LINHA} : 0.0]]></variableExpression>
		</variable>
		<variable name="TOTAL_TOTAL_QTD" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{TIPO}.equals("QTD") ? $V{TOTAL_LINHA} : 0.0]]></variableExpression>
		</variable>
	</subDataset>
