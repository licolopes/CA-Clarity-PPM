 TIPO_DEMANDA || ' R$' TIPO_DEMANDA
, (SUM(NVL(CORPORATIVO,0)))/1000 CORPORATIVO
, (SUM(NVL(NORTE,0)))/1000 NORTE
, (SUM(NVL(SUL,0)))/1000 SUL
, 'VAL' TIPO
, CASE WHEN TIPO_DEMANDA = 'CAPEX' THEN 1 ELSE 10 END ORDEM
FROM
(
	SELECT DISTINCT
		INVI.CODE INV_CODE
		, TIPO_DEMANDA.NAME TIPO_DEMANDA 
		, CASE WHEN OPERACAO.NAME = 'Corporativo' THEN NVL(ROUND(OCP.all_cst_total_fct,0),0) END CORPORATIVO
		, CASE WHEN OPERACAO.NAME = 'Operação Norte' THEN NVL(ROUND(OCP.all_cst_total_fct,0),0) END NORTE
		, CASE WHEN OPERACAO.NAME = 'Operação Sul' THEN NVL(ROUND(OCP.all_cst_total_fct,0),0) END SUL
	FROM 
		INV_INVESTMENTS INVI
		LEFT JOIN ODF_CA_INV OCINV ON INVI.ID = OCINV.ID
		LEFT JOIN ODF_CA_PROJECT OCPRJ ON OCPRJ.ID = INVI.ID
		LEFT JOIN ODF_CA_IDEA OCID ON OCID.ID = INVI.ID AND INVI.STATUS !=8
		LEFT JOIN CMN_LOOKUPS_V TIPO_DEMANDA ON (TIPO_DEMANDA.LOOKUP_CODE = OCPRJ.all_tipo_demanda OR TIPO_DEMANDA.LOOKUP_CODE = OCID.all_tipo_demanda) AND TIPO_DEMANDA.LANGUAGE_CODE = 'pt' AND TIPO_DEMANDA.LOOKUP_TYPE = 'ALL_TIPO_DEMANDA'
		LEFT JOIN CMN_LOOKUPS_V OPERACAO ON OPERACAO.LOOKUP_CODE = OCINV.all_area AND OPERACAO.LANGUAGE_CODE = 'pt' AND OPERACAO.LOOKUP_TYPE = 'ALL_LKP_AREAS'
		LEFT JOIN FIN_PLANS FP ON FP.OBJECT_ID = INVI.ID AND FP.PLAN_TYPE_CODE = 'FORECAST'
		LEFT JOIN FIN_COST_PLAN_DETAILS FPD on FP.ID = FPD.PLAN_ID
		LEFT JOIN ODF_SSL_CST_DTL_COST FP_COST ON FPD.ID = FP_COST.PRJ_OBJECT_ID
