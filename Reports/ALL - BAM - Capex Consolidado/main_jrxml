TIPO_DEMANDA]]>
		</queryString>
		<field name="TIPO_DEMANDA" class="java.lang.String"/>
		<field name="CORPORATIVO" class="java.math.BigDecimal"/>
		<field name="NORTE" class="java.math.BigDecimal"/>
		<field name="SUL" class="java.math.BigDecimal"/>
		<field name="TIPO" class="java.lang.String"/>
		<field name="ORDEM" class="java.math.BigDecimal"/>
		<sortField name="ORDEM"/>
		<variable name="TOTAL_NORTE_FCT_VAL" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[AND($F{TIPO}.equals("VAL"),$F{TIPO_DEMANDA}.equals("Forecast")) ? $F{NORTE} : 0.0]]></variableExpression>
		</variable>
		<variable name="TOTAL_NORTE_ORC_VAL" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[AND($F{TIPO}.equals("VAL"),$F{TIPO_DEMANDA}.equals("Orçamento")) ? $F{NORTE} : 0.0]]></variableExpression>
		</variable>
		<variable name="TOTAL_SUL_FCT_VAL" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[AND($F{TIPO}.equals("VAL"),$F{TIPO_DEMANDA}.equals("Forecast")) ? $F{SUL} : 0.0]]></variableExpression>
		</variable>
		<variable name="TOTAL_SUL_ORC_VAL" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[AND($F{TIPO}.equals("VAL"),$F{TIPO_DEMANDA}.equals("Orçamento")) ? $F{SUL} : 0.0]]></variableExpression>
		</variable>
		<variable name="TOTAL_CORP_FCT_VAL" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[AND($F{TIPO}.equals("VAL"),$F{TIPO_DEMANDA}.equals("Forecast")) ? $F{CORPORATIVO} : 0.0]]></variableExpression>
		</variable>
		<variable name="TOTAL_CORP_ORC_VAL" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[AND($F{TIPO}.equals("VAL"),$F{TIPO_DEMANDA}.equals("Orçamento")) ? $F{CORPORATIVO} : 0.0]]></variableExpression>
		</variable>
		<variable name="TOTAL_LINHA" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$F{NORTE}.add($F{SUL}).add($F{CORPORATIVO})]]></variableExpression>
		</variable>
		<variable name="SUB_NORTE" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$V{TOTAL_NORTE_ORC_VAL}.subtract( $V{TOTAL_NORTE_FCT_VAL} )]]></variableExpression>
		</variable>
		<variable name="SUB_SUL" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$V{TOTAL_SUL_ORC_VAL}.subtract( $V{TOTAL_SUL_FCT_VAL} )]]></variableExpression>
		</variable>
		<variable name="SUB_CORPORATIVO" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$V{TOTAL_CORP_ORC_VAL}.subtract( $V{TOTAL_CORP_FCT_VAL} )]]></variableExpression>
		</variable>
		<variable name="SUB_TOTAL" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$V{SUB_NORTE}.add($V{SUB_SUL}).add($V{SUB_CORPORATIVO})]]></variableExpression>
		</variable>
	</subDataset>
	<subDataset name="FORECAST" uuid="db7756b3-b39e-457f-ab85-854c052dafa2">
